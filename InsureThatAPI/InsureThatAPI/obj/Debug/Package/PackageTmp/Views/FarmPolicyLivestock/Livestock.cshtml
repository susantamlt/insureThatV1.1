@model InsureThatAPI.Models.FPLivestock
@{
    ViewBag.Title = "Livestock";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 policy-indiv-head">
    <h4><span>FARM PROTECTION POLICY</span> / LIVESTOCK</h4>
</div>
<div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 endorsmentview">
    <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 policy-full-head">
        <h2>LIVESTOCK</h2>
    </div>
    <div class="col-md-5 col-sm-5 col-xs-12 pull-right">
        <div class="pull-right">
            <button type="button" href="#savepolicy" role="button" data-target="#savepolicy" data-toggle="modal" class="button-3d nomargin savepolicyP tab-linker" id="savepolicyP" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Save Policy</button>
            <button type="button" href="#referralListModel" role="button" data-target="#referralListModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="referralList" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Referral List</button>
            <button type="button" href="#premiumModel" role="button" data-target="#premiumModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="premium" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Premium</button>
        </div>
    </div>
    <div id="savepolicy" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="savepolicy" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Save Policy</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <p>Do you wish to save your policy till now and continue later.</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                    <button class="btn btn-default no-border savepolicyP" data-dismiss="modal" aria-hidden="true"> Save </button>
                </div>
            </div>
        </div>
    </div>
    <div id="referralListModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="referralListModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="pull-left">Referral List</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    @if (Model.Referels != null && Model.Referels.Count() > 0)
                    {
                        for (int j = 0; j < Model.Referels.Count(); j++)
                        {
                            @Model.Referels[j]<br />
                        }
                    }
                    else
                    {
                        <p>No Referral List</p>
                    }
                    <p id="referralList-p"></p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div id="premiumModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="premiumModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Premium Details</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table" style=" width: 100%; ">
                        <thead>
                            <tr>
                                <th class="space_left">Section Name</th>
                                <th class="space_left">Base</th>
                                <th class="space_left">Fsl</th>
                                <th class="space_left">Gst</th>
                                <th class="space_left">Stamp Duty</th>
                                <th class="space_right">Gross</th>
                            </tr>
                        </thead>
                        <tbody id="premiumModel-p-b">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tbody>
                        <tfoot id="premiumModel-p-f">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 success" id="savepolicyresult"></div>
    @using (Ajax.BeginForm("Livestock", "FarmPolicyLivestock", new { cid = @ViewBag.cid }, new AjaxOptions()
    {
        HttpMethod = "POST"
    }, new { @id = "livestockID", @name = "livestockName" }))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(p => p.CustomerId, new { @id = "ciid" })
        @Html.HiddenFor(p => p.PcId, new { id = "pcidd" })
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <h4>Part A – Death - <a href="#costumModal2" role="button" data-target="#costumModal2" data-toggle="modal">For more information click here</a></h4>
            <div id="costumModal2" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h3 class="pull-left">Part A - Death Cover </h3>
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                <img src="~/images/close-icon.png" />
                            </button>
                        </div>
                        <div class="modal-body">
                            <p>
                                Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.
                            </p>
                            <p> Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.</p>
                        </div>
                        <div class="modal-footer">
                            <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                        </div>
                    </div>
                </div>
            </div>
            <h5>Please provide the details for each animal to be covered:</h5>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Class of animal</p></div>
            <div class="col-md-6">
                @Html.DropDownListFor(Model => Model.ClassOfAnimalFPObj.ClassofAnimal, @Model.ClassOfAnimalFPObj.ClassOfAnimalList, "-- Select --", new { @class = "commondataselection form-control form-border-bottom description", @id = "classofanimal", @data_eiid = @Model.ClassOfAnimalFPObj.EiId })
                @Html.HiddenFor(p => p.ClassOfAnimalFPObj.EiId)
                <label id="ClassOfAnimalFPObj.ClassofAnimal-error" class="error" for="ClassOfAnimalFPObj.ClassofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Type</p></div>
            <div class="col-md-6">
                @Html.DropDownListFor(Model => Model.TypeOfAnimalFPObj.TypeofAnimal, @Model.TypeOfAnimalFPObj.TypeOfAnimalList, "-- Select --", new { @class = "commondataselection form-control form-border-bottom description", @id = "typeofanimal", @data_eiid = @Model.TypeOfAnimalFPObj.EiId })
                @Html.HiddenFor(p => p.TypeOfAnimalFPObj.EiId)
                <label id="TypeOfAnimalFPObj.TypeofAnimal-error" class="error" for="TypeOfAnimalFPObj.TypeofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>What breed is your animal?</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.BreedOfAnimalFPObj.BreedofAnimal, new { @class = "commondatatest form-control form-border-bottom", @id = "breedofanimal", @data_eiid = @Model.BreedOfAnimalFPObj.EiId, @data_regex = 'A' })
                @Html.HiddenFor(p => p.BreedOfAnimalFPObj.EiId)
                <label id="BreedOfAnimalFPObj.BreedofAnimal-error" class="error" for="BreedOfAnimalFPObj.BreedofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>What is the age of the animal?</p></div>
            <div class="col-md-6">
                @Html.DropDownListFor(Model => Model.AgeOfAnimalFPObj.AgeofAnimal, @Model.AgeOfAnimalFPObj.AgeOfAnimalList, "-- Select --", new { @class = "commondataselection form-control form-border-bottom description", @id = "ageofanimal", @data_eiid = @Model.AgeOfAnimalFPObj.EiId })
                @Html.HiddenFor(p => p.AgeOfAnimalFPObj.EiId)
                <label id="AgeOfAnimalFPObj.AgeofAnimal-error" class="error" for="AgeOfAnimalFPObj.AgeofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>What colour are they?</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.ColourOfAnimalFPObj.ColourofAnimal, new { @class = "commondatatest form-control form-border-bottom", @id = "colourofanimal", @data_eiid = @Model.ColourOfAnimalFPObj.EiId, @data_regex = 'A' })
                @Html.HiddenFor(p => p.ColourOfAnimalFPObj.EiId)
                <label id="ColourOfAnimalFPObj.ColourofAnimal-error" class="error" for="ColourOfAnimalFPObj.ColourofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>What is your animal used for?</p></div>
            <div class="col-md-6">
                @Html.DropDownListFor(Model => Model.UseOfAnimalFPObj.UseofAnimal, @Model.UseOfAnimalFPObj.UseOfAnimalList, "-- Select --", new { @class = "commondataselection form-control form-border-bottom description", @id = "useofanimal", @data_eiid = @Model.UseOfAnimalFPObj.EiId })
                @Html.HiddenFor(p => p.UseOfAnimalFPObj.EiId)
                <label id="UseOfAnimalFPObj.UseofAnimal-error" class="error" for="UseOfAnimalFPObj.UseofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Describe their brand</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.DescBrandOfAnimalFPObj.BrandofAnimal, new { @class = "commondatatest form-control form-border-bottom", @id = "brandofanimaldes", @data_eiid = @Model.DescBrandOfAnimalFPObj.EiId, @data_regex = 'A' })
                @Html.HiddenFor(p => p.DescBrandOfAnimalFPObj.EiId)
                <label id="DescBrandOfAnimalFPObj.BrandofAnimal-error" class="error" for="DescBrandOfAnimalFPObj.BrandofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Describe their markings</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.DescMarksOfAnimalFPObj.MarkingsofAnimal, new { @class = "commondatatest form-control form-border-bottom", @id = "markingsofanimal", @data_eiid = @Model.DescMarksOfAnimalFPObj.EiId, @data_regex = 'A' })
                @Html.HiddenFor(p => p.DescMarksOfAnimalFPObj.EiId)
                <label id="DescMarksOfAnimalFPObj.MarkingsofAnimal-error" class="error" for="DescMarksOfAnimalFPObj.MarkingsofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Is the animal in sound health?</p></div>
            <div class="col-md-6">
                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                    @Html.RadioButtonFor(m => m.OptSoundHealthofAnimalFPObj.HealthofAnimal, '1', new { @class = "commondataradio buildingtype", @id = "buildingtype", @checked = "checked", @data_eiid = @Model.OptSoundHealthofAnimalFPObj.EiId })
                    <label for="radio1"><span><span></span></span>Yes</label>
                </div>
                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                    @Html.RadioButtonFor(m => m.OptSoundHealthofAnimalFPObj.HealthofAnimal, '0', new { @class = "commondataradio buildingtype", @id = "buildingtype1", @data_eiid = @Model.OptSoundHealthofAnimalFPObj.EiId })
                    <label for="radio1"><span><span></span></span>No</label>
                </div>
                <label id="OptSoundHealthofAnimalFPObj.HealthofAnimal-error" class="error" for="OptSoundHealthofAnimalFPObj.HealthofAnimal"></label>
                @Html.HiddenFor(p => p.OptSoundHealthofAnimalFPObj.EiId)
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>If no, please provide details</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.DescSoundHealthofAnimalFPObj.Description, new { @class = "commondatatest form-control form-border-bottom", @id = "helthofdescription", @data_eiid = @Model.DescSoundHealthofAnimalFPObj.EiId, @data_regex = 'A' })
                @Html.HiddenFor(p => p.DescSoundHealthofAnimalFPObj.EiId)
                <label id="DescSoundHealthofAnimalFPObj.Description-error" class="error" for="DescSoundHealthofAnimalFPObj.Description"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Are there any contagious or infectious diseases in any of your stock?</p></div>
            <div class="col-md-6">
                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                    @Html.RadioButtonFor(m => m.OptDiseaseOfAnimalFPObj.DiseaseofAnimal, '1', new { @class = "commondataradio buildingtype", @id = "buildingtype", @data_eiid = @Model.OptDiseaseOfAnimalFPObj.EiId })
                    <label for="radio1"><span><span></span></span>Yes</label>
                </div>
                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                    @Html.RadioButtonFor(m => m.OptDiseaseOfAnimalFPObj.DiseaseofAnimal, '0', new { @class = "commondataradio buildingtype", @id = "buildingtype1", @checked = "checked", @data_eiid = @Model.OptDiseaseOfAnimalFPObj.EiId })
                    <label for="radio1"><span><span></span></span>No</label>
                </div>
                <label id="OptDiseaseOfAnimalFPObj.DiseaseofAnimal-error" class="error" for="OptDiseaseOfAnimalFPObj.DiseaseofAnimal"></label>
                @Html.HiddenFor(p => p.OptDiseaseOfAnimalFPObj.EiId)
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>If no, please provide details</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.DescDiseaseOfAnimalFPObj.Description, new { @class = "commondatatest form-control form-border-bottom", @id = "diseasesofanimal", @data_eiid = @Model.DescDiseaseOfAnimalFPObj.EiId, @data_regex = 'A' })
                @Html.HiddenFor(p => p.DescDiseaseOfAnimalFPObj.EiId)
                <label id="DescDiseaseOfAnimalFPObj.Description-error" class="error" for="DescDiseaseOfAnimalFPObj.Description"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Is the animal syndicated?</p></div>
            <div class="col-md-6">
                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                    @Html.RadioButtonFor(m => m.OptAnimalSyndicatedFPObj.AnimalSyndicated, '1', new { @class = "commondataradio buildingtype", @id = "OptAnimalSyndicatedFP1", @data_eiid = @Model.OptAnimalSyndicatedFPObj.EiId })
                    <label for="radio1"><span><span></span></span>Yes</label>
                </div>
                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                    @Html.RadioButtonFor(m => m.OptAnimalSyndicatedFPObj.AnimalSyndicated, '0', new { @class = "commondataradio buildingtype", @id = "OptAnimalSyndicatedFP2", @checked = "checked", @data_eiid = @Model.OptAnimalSyndicatedFPObj.EiId })
                    <label for="radio1"><span><span></span></span>No</label>
                </div>
                <label id="OptAnimalSyndicatedFPObj.AnimalSyndicated-error" class="error" for="OptAnimalSyndicatedFPObj.AnimalSyndicated"></label>
                @Html.HiddenFor(p => p.OptAnimalSyndicatedFPObj.EiId)
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>For how much do you wish to insure your animal?</p></div>
            <div class="col-md-6">
                $@Html.TextBoxFor(p => p.SumInsuredLivestockFPObj.SumInsured, new { @style = "width:95%; display:inline-block;", @class = "commondatatest form-control form-border-bottom", @id = "suminsuredlivestock", @data_eiid = @Model.SumInsuredLivestockFPObj.EiId, @data_regex = 'N' })
                @Html.HiddenFor(p => p.SumInsuredLivestockFPObj.EiId)
                <label id="SumInsuredLivestockFPObj.SumInsured-error" class="error" for="SumInsuredLivestockFPObj.SumInsured"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10 ">
            <div class="col-md-6"><p>You can choose the following option extras.  Click on each option to obtain further details</p></div>
            <div class="col-md-6" id="obtainFurtherDetailsCheckbox">
                <div class="policy-loc-row col-md-6 col-sm-6 col-xs-6">@Html.CheckBox("obtainFurtherDetails", new { @class = "buildingtype", @id = "infertility", @value = @Model.OptInfertilityFPObj.EiId, @data_eiid = @Model.OptInfertilityFPObj.EiId })<label for="checkbox1"><span><span></span></span><a href="#costumModal3" role="button" data-target="#costumModal3" data-toggle="modal">Infertility</a></label></div>
                <div id="costumModal3" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h3 class="pull-left">Infertility</h3>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                    <img src="~/images/close-icon.png" />
                                </button>
                            </div>
                            <div class="modal-body">
                                <p>
                                    Insure that helps livestock farmers worldwide protect their assets from risks such as mortality, infertility, theft and disease.
                                </p>
                                <p> We use the know-how of industry experts, such as livestock agents and vets, to assess risks and design fairly priced products</p>
                            </div>
                            <div class="modal-footer">
                                <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="policy-loc-row col-md-6 col-sm-6 col-xs-6">@Html.CheckBox("obtainFurtherDetails", new { @class = "buildingtype", @id = "LossofUse", @value = @Model.OptLossofUseLivestockFPObj.EiId, @data_eiid = @Model.OptLossofUseLivestockFPObj.EiId })<label for="checkbox2"><span><span></span></span><a href="#costumModal4" role="button" data-target="#costumModal4" data-toggle="modal">Loss of Use</a></label></div>
                <div id="costumModal4" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h3 class="pull-left">Loss of Use</h3>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                    <img src="~/images/close-icon.png" />
                                </button>
                            </div>
                            <div class="modal-body">
                                <p>
                                    Introducing full mortality and loss of use coverage, the best protection we can offer for your significant investment.
                                </p>
                                <p> Covers total and permanent infertility resulting from an accidental breeding injury. Requires a veterinarian statement of soundness, unless bound date of auction purchase.</p>
                            </div>
                            <div class="modal-footer">
                                <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="policy-loc-row col-md-6 col-sm-6 col-xs-6">@Html.CheckBox("obtainFurtherDetails", new { @class = "buildingtype", @id = "theft", @value = @Model.OptTheftLivestockFPObj.EiId, @data_eiid = @Model.OptTheftLivestockFPObj.EiId })<label for="checkbox3"><span><span></span></span><a href="#costumModal5" role="button" data-target="#costumModal5" data-toggle="modal">Theft</a></label></div>
                <div id="costumModal5" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h3 class="pull-left">Theft</h3>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                    <img src="~/images/close-icon.png" />
                                </button>
                            </div>
                            <div class="modal-body">
                                <p>
                                    Raising livestock and poultry to sell can be unpredictable and risky. That's why a solid and affordable livestock or poultry insurance policy is a necessit.
                                </p>
                                <p> This insurance protects your investment from those unexpected events and accidents that can decimate your animals and your livelihood.</p>
                            </div>
                            <div class="modal-footer">
                                <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="policy-loc-row col-md-6 col-sm-6 col-xs-6">@Html.CheckBox("obtainFurtherDetails", new { @class = "buildingtype", @id = "UnbornFoal", @value = @Model.OptUnbornFoalFPObj.EiId, @data_eiid = @Model.OptUnbornFoalFPObj.EiId })<label for="checkbox4"><span><span></span></span><a href="#costumModal6" role="button" data-target="#costumModal6" data-toggle="modal">Unborn Foal</a></label></div>
                <div id="costumModal6" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h3 class="pull-left">Unborn Foal</h3>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                    <img src="~/images/close-icon.png" />
                                </button>
                            </div>
                            <div class="modal-body">
                                <p>
                                    Prospective Foal Insurance indemnifies you in the event that your unborn foal is not born alive or does not survive to a certain age.
                                </p>
                                <p> Most commonly, cover is arranged from the date that the mare tests pregnant 45 days after last service, through until the foal is born and reaches 30 days of age.
                            </div>

                                <span class="policy-loc-row">@Html.RadioButtonFor(m => m.WatertightObj.Watertight, '1', new { @class = "commondataradio vlidationInsure", @checked = "checked", @data_eiid = @Model.WatertightObj.EiId })<label for="watertight1"><span><span></span></span>Yes</label></span>

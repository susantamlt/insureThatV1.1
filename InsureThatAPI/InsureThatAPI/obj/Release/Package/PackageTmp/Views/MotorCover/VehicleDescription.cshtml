        $("#tname-tr-" + countId + ' #gender').attr('id', 'gender' + countId);
        $("#tname-tr-" + countId + ' #convictions').attr('id', 'convictions' + countId);
        $("#tname-tr-" + countId + ' #percentage').attr('id', 'percentage' + countId);
    }

@model InsureThatAPI.Models.MCVehicleDescription
@{
    ViewBag.Title = "Vehicle Description";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 policy-indiv-head">
    <h4><span>RURAL LIFESTYLE POLICY</span> / MOTOR</h4>
</div>
<div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 endorsmentview">
    <div class="col-md-3 col-sm-3 col-xs-12 pull-right">
        <div class="pull-right">
            <button type="button" href="#referralListModel" role="button" data-target="#referralListModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="referralList" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Referral List</button>
            <button type="button" href="#premiumModel" role="button" data-target="#premiumModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="premium" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Premium</button>
        </div>
    </div>
    <div id="referralListModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="referralListModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="pull-left">Referral List</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <p id="referralList-p">No Referral List</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div id="premiumModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="premiumModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Premium Details</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table" style=" width: 100%; ">
                        <thead>
                            <tr>
                                <th class="space_left">Section Name</th>
                                <th class="space_left">Base</th>
                                <th class="space_left">Fsl</th>
                                <th class="space_left">Gst</th>
                                <th class="space_left">Stamp Duty</th>
                                <th class="space_right">Gross</th>
                            </tr>
                        </thead>
                        <tbody id="premiumModel-p-b">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tbody>
                        <tfoot id="premiumModel-p-f">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div class="mt-40 mb-40 row"></div>
    <div id="processTabs">
        <ul class="process-steps clearfix col-sm-12" style="padding:0px 0px 0px 10px !important;">
            <li>
                <a href="javascript:void(0)" id="ptb1">Vehicle Description</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb2">Additional Details</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb3">Drivers</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb4">Cover Details</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb5">Optional Extras and Excesses</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb6">Interested Parties</a>
            </li>
        </ul>
        @using (Ajax.BeginForm("VehicleDescription", "MotorCover", new { cid = @ViewBag.CustomerId }, new AjaxOptions()
        {
            HttpMethod = "POST"
        }, new { @id = "vehicleDescription", @name = "vehicleDescription" }))
        {
            @Html.AntiForgeryToken()

            @Html.HiddenFor(p => p.CustomerId)
        
            @Html.HiddenFor(p => p.ApiKey, new { @id = "apisecret" })
            <div id="homeBuildContain">
                <div id="ptab1">
                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 policy-full-head"><h2>VEHICLE DESCRIPTION</h2></div>
                    <div class="mt-10 mb-10 col-md-12">
                        <h5>Glass's Listed Vehicles</h5>
                    </div>
                    <div class="mt-10 mb-10 col-md-12 ">
                        <p class="col-md-3 col-sm-12 col-xs-12 policy-loc-row">Category</p>
                        <div class="col-md-9 col-sm-12 col-xs-12">
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.LscategoryObj.Category, '0', new { @class = "commondataradio", @data_eiid = @Model.LscategoryObj.EiId, @id = "buildingtype" })<label for="radio1"><span><span></span></span>Passenger</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.LscategoryObj.Category, '1', new { @class = "commondataradio", @data_eiid = @Model.LscategoryObj.EiId, @id = "holidayhome" })<label for="radio1"><span><span></span></span>Light Commercial</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.LscategoryObj.Category, '2', new { @class = "commondataradio", @data_eiid = @Model.LscategoryObj.EiId, @id = "other" })<label for="radio1"><span><span></span></span>Heavy</label></div>
                            @Html.HiddenFor(p => p.LscategoryObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What is the make of your vehicle</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.McmakeObj.Make, @Model.McmakeObj.MakeList, new { @class = "form-control form-border-bottom vehicle commondataselection", @data_eiid = @Model.McmakeObj.EiId, @id = "vehiclemake" })
                            @Html.HiddenFor(p => p.McmakeObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What year was it manufactured</p></div>
                        <div class="col-md-6">
                            @Html.TextBoxFor(p => p.McyearObj.Year, new { @class = "form-control form-border-bottom year vehicle commondatatest", @id = "year1", @data_eiid = @Model.McyearObj.EiId })
                            @Html.HiddenFor(p => p.McyearObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What is the family</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.MCfamilyObj.Family, @Model.MCfamilyObj.FamilyList, new { @class = "form-control form-border-bottom vehicle commondataselection", @id = "family1", @data_eiid = @Model.MCfamilyObj.EiId })
                            @Html.HiddenFor(p => p.MCfamilyObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>Select Correct Details</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.MCscdObj.Scd, Enumerable.Empty<SelectListItem>(), "--Select--", new { @class = "form-control form-border-bottom vehicle commondataselection", @id = "details1", @data_eiid = @Model.MCscdObj.EiId })
                            @Html.HiddenFor(p => p.MCscdObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>Estimated retail value * *</p></div>
                        <div class="col-md-6">
                            <input type="text" name="retailvalue" id="retailvalue" class="form-control form-border-bottom">
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <h5>Farm Machinery</h5>
                    </div>

                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What is the make of your vehicle</p></div>
                        <div class="col-md-6">
                            @Html.TextBoxFor(p => p.FmmcmakeObj.FmMake, new { @class = "form-control form-border-bottom vehicle commondatatest", @id = "vehiclemake2", @data_eiid = @Model.FmmcmakeObj.EiId })
                            @Html.HiddenFor(p => p.FmmcmakeObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What year was it manufactured</p></div>
                        <div class="col-md-6">
                            @Html.TextBoxFor(p => p.FmmcyearObj.FmYear, new { @class = "form-control form-border-bottom year vehicle commondatatest", @id = "year2", @data_eiid = @Model.FmmcyearObj.EiId })
                            @Html.HiddenFor(p => p.FmmcyearObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What is the family</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.FmmctypeObj.FmFamily, @Model.FmmctypeObj.FmFamilyList, new { @class = "form-control form-border-bottom vehicle commondataselection", @id = "family2", @data_eiid = @Model.FmmctypeObj.EiId })
                            @Html.HiddenFor(p => p.FmmctypeObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>Select Correct Details</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.FmmcscdObj.FmScd, Enumerable.Empty<SelectListItem>(), "--Select--", new { @class = "form-control form-border-bottom vehicle commondataselection", @id = "details2", @data_eiid = @Model.FmmcscdObj.EiId })
                            @Html.HiddenFor(p => p.FmmcscdObj.EiId)
                        </div>
                    </div>
                    <div class="col-md-12"><p class="mini-text">Is the Farm Machinery to be insured operated and maintained in accordance with the manufacturer’s instructions as stated in the owner’s manual?</p></div>
                    <div class="col-md-6 pull-right text-right mt-20">
                        <button type="button" class="button-3d nomargin tab-linker quot-next-btn homedescrtn" id="ptabbtn1">Next</button>
                    </div>
                </div>
                <div id="ptab2">
                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 policy-full-head"><h2>ADDITIONAL DESCRIPTION</h2></div>
                    <div class="mt-10 mb-10 col-md-12 ">
                        <p class="col-md-4 col-sm-4 col-xs-12 policy-loc-row">Where is Vehicle normally kept at night?</p>
                        <div class="col-md-8 col-sm-8 col-xs-12">
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '0', new { @class = "commondataradio", @id = "vehclekept1", @data_eiid = @Model.KeptnightObj.EiId, @checked = "checked" })<label for="radio1"><span><span></span></span>Garage</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '1', new { @class = "commondataradio", @id = "vehclekept2", @data_eiid = @Model.KeptnightObj.EiId })<label for="radio1"><span><span></span></span>Driveway</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '2', new { @class = "commondataradio", @id = "vehclekept3", @data_eiid = @Model.KeptnightObj.EiId })<label for="radio1"><span><span></span></span>Carport</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '3', new { @class = "commondataradio", @id = "vehclekept4", @data_eiid = @Model.KeptnightObj.EiId })<label for="radio1"><span><span></span></span>Street</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '4', new { @class = "commondataradio", @id = "vehclekept5", @data_eiid = @Model.KeptnightObj.EiId })<label for="radio1"><span><span></span></span>Shed</label></div>
                            @Html.HiddenFor(p => p.KeptnightObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Address</p></div>
                        <div class="col-md-8">
                            @Html.DropDownListFor(p => p.AdaddressObj.Address, @Model.AdaddressObj.AddressList, new { @class = "commondataselection form-control form-border-bottom", @id = "Address", @data_eiid = @Model.AdaddressObj.EiId })
                            @Html.HiddenFor(p => p.AdaddressObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Is Vehicle registered?</p></div>
                        <div class="col-md-8">
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.VregisterObj.Register, '1', new { @class = "commondataradio", @id = "yes", @data_eiid = @Model.VregisterObj.EiId, @checked = "checked" })<label for="radio1"><span><span></span></span>Yes</label></div>
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.VregisterObj.Register, '0', new { @class = "commondataradio", @id = "no", @data_eiid = @Model.VregisterObj.EiId })<label for="radio1"><span><span></span></span>No</label></div>
                            @Html.HiddenFor(p => p.VregisterObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Registration number</p></div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(p => p.RnumberObj.Rnumber, new { @class = "form-control form-border-bottom commondatatest", @id = "rnumber", @data_eiid = @Model.RnumberObj.EiId })
                            @Html.HiddenFor(p => p.RnumberObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>VIN/chassis number</p></div>
                        <div class="col-md-8">
                            @Html.DropDownListFor(p => p.VnumberObj.Vnumber, @Model.VnumberObj.VnumberList, new { @class = "commondataselection form-control form-border-bottom", @id = "chassis", @data_eiid = @Model.VnumberObj.EiId })
                            @Html.HiddenFor(p => p.AdaddressObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Engine number</p></div>
                        <div class="col-md-8">
                            @Html.DropDownListFor(p => p.EnumberObj.Enumber, @Model.EnumberObj.EnumberList, new { @class = "commondataselection form-control form-border-bottom", @id = "enumber", @data_eiid = @Model.EnumberObj.EiId })
                            @Html.HiddenFor(p => p.EnumberObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Has Vehicle been modified?</p></div>
                        <div class="col-md-8">
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.VmodifiedObj.Vmodified, '1', new { @class = "commondataradio", @id = "vehicle1", @data_eiid = @Model.VmodifiedObj.EiId, @checked = "checked" })<label for="radio1"><span><span></span></span>Yes</label></div>
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.VmodifiedObj.Vmodified, '0', new { @class = "commondataradio", @id = "vehicle2", @data_eiid = @Model.VmodifiedObj.EiId })<label for="radio1"><span><span></span></span>No</label></div>
                            @Html.HiddenFor(p => p.VmodifiedObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>If yes, please describe modifications</p></div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(p => p.DmodifiedObj.Dmodified, new { @class = "form-control form-border-bottom commondatatest", @id = "describe", @data_eiid = @Model.DmodifiedObj.EiId })
                            @Html.HiddenFor(p => p.DmodifiedObj.EiId)
                            <p class="mini-text">(These will require approval from us before accepting your quotation)</p>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>What security features are installed?</p></div>
                        <div class="col-md-8">
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.SFinstalledObj.Installed, new { @class = "commondatacheckbox", @id = "alarm", @data_eiid = @Model.SFinstalledObj.EiId })<label for="checkbox1"><span><span></span></span>Alarm</label></div>
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.SFinstalledObj.Installed, new { @class = "commondatacheckbox", @id = "immobiliser", @data_eiid = @Model.SFinstalledObj.EiId })<label for="checkbox1"><span><span></span></span>Engine immobiliser</label></div>
                            @Html.HiddenFor(p => p.SFinstalledObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>What is the Vehicle used for?</p></div>
                        <div class="col-md-8">
                            <div class="col-md-12 col-sm-12 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.VusedObj.Vused, new { @class = "commondatacheckbox", @id = "private", @data_eiid = @Model.VusedObj.EiId })<label for="checkbox1"><span><span></span></span>Private Use</label></div>
                            <div class="col-md-12 col-sm-12 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.VusedObj.Vused, new { @class = "commondatacheckbox", @id = "farm", @data_eiid = @Model.VusedObj.EiId })<label for="checkbox1"><span><span></span></span>Farm use (no farm contracting)</label></div>
                            <div class="col-md-12 col-sm-12 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.VusedObj.Vused, new { @class = "commondatacheckbox", @id = "business", @data_eiid = @Model.VusedObj.EiId })<label for="checkbox1"><span><span></span></span>Business Use (no farm contracting)</label></div>
                            @Html.HiddenFor(p => p.VusedObj.EiId)
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <p>
                            If your vehicle is a truck or trailer (including horse floats) What is the carrying capacity
                            @Html.TextBoxFor(p => p.CcapacityObj.Ccapacity, new { @class = "form-control form-inline field-length-xs commondatatest", @id = "tones", @data_eiid = @Model.CcapacityObj.EiId })
                            @Html.HiddenFor(p => p.CcapacityObj.EiId) tonnes.
                        </p>
                        <p class="mini-text">(Vehicles over 10 tonnes will require approval.)</p>
                    </div>
                    <div class="col-md-6 pull-right text-right">
                        <a href="javascript:void(0)" id="ptb12" class="button-3d nomargin tab-linker quot-prev-btn" rel="1">Prev</a>
                        <img src="~/images/button-devider.png">
                        <button type="button" class="button-3d nomargin tab-linker quot-next-btn" id="ptabbtn2">Next</button>
                    </div>
                </div>
                <div id="ptab3">
                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 policy-full-head"><h2>DRIVERS</h2></div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-12 table-8">
                            <table class="table" style=" width: 100%; ">
                                <thead>
                                    <tr>
                                        <th class="space_left">Name</th>
                                        <th class="space_left">Date of Birth</th>
                                        <th class="space_left">Gender</th>
                                        <th class="space_left">Accidents and Motoring Infringements and Convictions</th>
                                        <th class="space_left">Percentage of use of vehicle</th>
                                        <th class="space_right">&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr id="tname-tr-1">
                                        <td class="no-border">
                                            @Html.TextBoxFor(p => p.DrivernameObj.Name, new { @class = "form-control form-border-bottom commondatatest", @data_itid = "0", @id = "drivername1", @data_eiid = @Model.DrivernameObj.EiId })
                                            @Html.HiddenFor(p => p.DrivernameObj.EiId)
                                        </td>
                                        <td class="no-border">
                                            @Html.TextBoxFor(p => p.DriverageObj.Age, new { @class = "form-control form-border-bottom commondatatest", @data_itid = "0", @id = "age1", @data_eiid = @Model.DriverageObj.EiId })
                                            @Html.HiddenFor(p => p.DriverageObj.EiId)
                                        </td>
                                        <td class="no-border">
                                            @Html.DropDownListFor(p => p.DrivergenderObj.Gender, @Model.DrivergenderObj.GenderList, new { @class = "gray-bg-underline form-control form-border-bottom field-length-sm commondataselection", @data_itid = "", @id = "gender1", @data_eiid = @Model.DrivergenderObj.EiId })
                                            @Html.HiddenFor(p => p.DrivergenderObj.EiId)
                                        </td>
                                        <td class="no-border">
                                            @Html.TextBoxFor(p => p.DriveramicObj.Amic, new { @class = "form-control form-border-bottom commondatatest", @data_itid = "0", @id = "convictions1", @data_eiid = @Model.DriveramicObj.EiId })
                                            @Html.HiddenFor(p => p.DriveramicObj.EiId)
                                        </td>
                                        <td class="no-border">
                                            @Html.TextBoxFor(p => p.UsevehicleObj.Usevehicle, new { @class = "form-control form-border-bottom commondatatest", @data_itid = "0", @id = "percentage1", @data_eiid = @Model.UsevehicleObj.EiId })
                                            @Html.HiddenFor(p => p.UsevehicleObj.EiId)
                                        </td>
                                        <td class="no-border" id="tname-1"><a href="javascript:void(0)" onclick="tnameAdd(1)" class="button-3d">+</a></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="col-md-6 pull-right text-right">
                        <a href="javascript:void(0)" id="ptb23" class="button-3d nomargin tab-linker quot-prev-btn" rel="2">Prev</a>
                        <img src="~/images/button-devider.png">
                        <button type="button" class="button-3d nomargin tab-linker quot-next-btn" id="ptabbtn3">Next</button>
                    </div>
                </div>
                <div id="ptab4">
                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 policy-full-head"><h2>COVER DETAILS</h2></div>

                "rlsBDhome": "Please fillup the Boat Details,",
                "rlsMDhome": "Please fillup the motor details,",
                "rlsBOhome": "Please fillup the boat operator,",
                "rlsCDhome": "Please fillup the cover details,",
                "rlsOPodhome": "Please fillup the options,",
            },
            onfocusout: function (element) {
                this.element(element);
            },
            submitHandler: function (form) {
                form.submit();
            }
        });
    });
</script>
@model InsureThatAPI.Models.FarmContents
@{
    ViewBag.Title = "Farm Contents / Farm Property Cover";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 policy-indiv-head">
    <h4><span>RURAL LIFESTYLE POLICY</span> / FARM PROPERTY</h4>
</div>
<div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 endorsmentview">
    <div class="col-md-3 col-sm-3 col-xs-12 pull-right">
        <div class="pull-right">
            <button type="button" href="#referralListModel" role="button" data-target="#referralListModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="referralList" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Referral List</button>
            <button type="button" href="#premiumModel" role="button" data-target="#premiumModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="premium" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Premium</button>
        </div>
    </div>
    <div id="referralListModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="referralListModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="pull-left">Referral List</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <p id="referralList-p">No Referral List</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div id="premiumModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="premiumModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Premium Details</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table" style=" width: 100%; ">
                        <thead>
                            <tr>
                                <th class="space_left">Section Name</th>
                                <th class="space_left">Base</th>
                                <th class="space_left">Fsl</th>
                                <th class="space_left">Gst</th>
                                <th class="space_left">Stamp Duty</th>
                                <th class="space_right">Gross</th>
                            </tr>
                        </thead>
                        <tbody id="premiumModel-p-b">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tbody>
                        <tfoot id="premiumModel-p-f">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div class="mt-40 mb-40 row"></div>
    <div id="processTabs">
        <ul class="process-steps clearfix col-sm-12">
            <li>
                <a href="javascript:void(0)" id="ptb1">Farm Structures and Farm Contents</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb2">Farm Machinery</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb3">Livestock</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb4">Harvested Crops and Beehives</a>
            </li>
        </ul>
        @using (Ajax.BeginForm("FarmContents", "Farm", new { cid = @ViewBag.cid }, new AjaxOptions()
        {
            HttpMethod = "POST"

        }, new { @id = "farmContents", @name = "farmContents" }))
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(p => p.CustomerId, new { @id = "ciid" })

            @Html.HiddenFor(p => p.PcId, new { id = "pcidd" })
            <div id="ptab1">
                <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 policy-full-head"><h2 style="font-size: 27px;">FARM STRUCTURES AND FARM CONTENTS</h2></div>
                <div class="mt-10 mb-10 col-md-12">
                    <h5>Farm Building Details</h5>
                    <p>What type of Farm Building do you need to cover?</p>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-12 table-8">
                        <table class="table" style=" width: 100%; ">
                            <thead>
                                <tr>
                                    <th class="space_left">Description</th>
                                    <th class="space_left">Year</th>
                                    <th class="space_left">Construction</th>
                                    <th class="space_left">Coolroom</th>
                                    <th class="space_left">Sum Insured</th>
                                    <th class="space_right">&nbsp;</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr id="tnameFC-tr-1">
                                    <td class="no-border">
                                        @Html.DropDownListFor(Model => Model.DescriptionFCObj.Description, @Model.DescriptionFCObj.DescriptionList, "-- Select --", new { @class = "form-control form-border-bottom description vlidationInsure commondataselection", @data_eiid = @Model.DescriptionFCObj.EiId, @data_itid = "0", @id = "description1" })
                                        @Html.HiddenFor(p => p.DescriptionFCObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.YearObj.Year, new { @class = "form-control form-border-bottom vlidationInsure year commondatatest", @data_eiid = @Model.YearObj.EiId, @data_itid = "0", @id = "year1", @readonly = "readonly" })
                                        @Html.HiddenFor(p => p.YearObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        @Html.DropDownListFor(Model => Model.MaterialsObj.Materials, @Model.MaterialsObj.MaterialsList, "-- Select --", new { @class = "form-control form-border-bottom vlidationInsure commondataselection", @data_eiid = @Model.MaterialsObj.EiId, @data_itid = "0", @id = "materials1" })
                                        @Html.HiddenFor(p => p.MaterialsObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        <div class="policy-loc-row">
                                            <input type="checkbox" name="CoolroomFcObj.Coolroom" id="coolroom1" class="vlidationInsure"><label for="checkbox1"><span><span></span></span></label>
                                        </div>
                                    </td>
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.SuminsuredObj.Suminsured, new { @class = "form-control form-border-bottom  vlidationInsure sumoftotalcover commondatatest", @data_eiid = @Model.SuminsuredObj.EiId, @data_itid = "0", @id = "suminsured1" })
                                        @Html.HiddenFor(p => p.SuminsuredObj.EiId)
                                    </td>
                                    <td class="no-border" id="tnameFC-1"><a href="javascript:void(0)" onclick="tnameAddFC(1)" class="button-3d">+</a></td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td class="no-border"><label id="DescriptionFCObj.Description-error" class="error" for="DescriptionFCObj.Description"></label></td>
                                    <td class="no-border"><label id="YearObj.Year-error" class="error" for="YearObj.Year"></label></td>
                                    <td class="no-border"><label id="MaterialsObj.Materials-error" class="error" for="MaterialsObj.Materials"></label></td>
                                    <td class="no-border"><label id="CoolroomFcObj.Coolroom-error" class="error" for="CoolroomFcObj.Coolroom"></label></td>
                                    <td class="no-border"><label id="SuminsuredObj.Suminsured-error" class="error" for="SuminsuredObj.Suminsured"></label></td>
                                    <td class="no-border">&nbsp;</td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <p class="col-md-8 col-sm-8 col-xs-12">Do you confirm that all Farm Structures to be insured are structurally sound, well maintained and have no unrepaired damage?</p>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.confirmfsObj.Confirm, '1', new { @class = "buildingtype vlidationInsure commondataradio", @data_eiid = @Model.confirmfsObj.EiId, @id = "buildingtype", @checked = "checked" })
                            <label for="radio1"><span><span></span></span>Yes</label>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.confirmfsObj.Confirm, '0', new { @class = "buildingtype vlidationInsure commondataradio", @data_eiid = @Model.confirmfsObj.EiId, @id = "buildingtype1" })
                            <label for="radio1"><span><span></span></span>No</label>
                        </div>
                        @Html.HiddenFor(p => p.confirmfsObj.EiId)
                        <label id="confirmfsObj.Confirm-error" class="error" for="confirmfsObj.Confirm"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-6"><p>Please advise the cost per kilometre to replace your fencing</p></div>
                    <div class="col-md-6">
                        $@Html.TextBoxFor(p => p.FarmfencingObj.Farmfencing, new { @style = "width:95%; display:inline-block;", @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.FarmfencingObj.EiId, @id = "farmfencing" })
                        @Html.HiddenFor(p => p.FarmfencingObj.EiId)
                        <label id="FarmfencingObj.Farmfencing-error" class="error" for="FarmfencingObj.Farmfencing"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-6"><p>What is the total replacement cost of fencing cover you would like?</p></div>
                    <div class="col-md-6">
                        @Html.TextBoxFor(p => p.FarmcencingTcObj.Totalcover, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.FarmcencingTcObj.EiId, @id = "totalcover" })
                        @Html.HiddenFor(p => p.FarmcencingTcObj.EiId)
                        <label id="FarmcencingTcObj.Totalcover-error" class="error" for="FarmcencingTcObj.Totalcover"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-6">
                        <p>
                            Please advise how much you would like the rest of your Farm Structures to be insured<br />
                            <span class="mini-text">(* includes covered ways, exterior light fittings, irrigation systems, pens, sheep dips, stockyards, pipes, cables, power and telephone poles, silos, solar power units, tanks and windmills)</span>
                        </p>
                    </div>
                    <div class="col-md-6">
                      $  @Html.TextBoxFor(p => p.FarmstructuresObj.Farmstructures, new { @style = "width:95%; display:inline-block;", @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.FarmstructuresObj.EiId, @id = "farmstructures" })
                        @Html.HiddenFor(p => p.FarmstructuresObj.EiId)
                        <label id="FarmstructuresObj.Farmstructures-error" class="error" for="FarmstructuresObj.Farmstructures"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-6"><p>How much cover do you need to replace your  Farm Contents</p></div>
                    <div class="col-md-6">
                        @Html.TextBoxFor(p => p.FarmContentFcObj.Farmcontents, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.FarmContentFcObj.EiId, @id = "farmstructuresFc" })
                        @Html.HiddenFor(p => p.FarmContentFcObj.EiId)
                        <label id="FarmContentFcObj.Farmcontents-error" class="error" for="FarmContentFcObj.Farmcontents"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12 ">
                    <div class="col-md-6"><p>Please select the excess you wish to pay</p></div>
                    <div class="col-md-3">
                        @Html.DropDownListFor(Model => Model.ExcessesFpcObj.Excess, @Model.ExcessesFpcObj.ExcessList, new { @class = "gray-bg-underline vlidationInsure form-control form-border-bottom field-length-sm commondataselection", @data_eiid = @Model.ExcessesFpcObj.EiId, @id = "excessFC" })
                        @Html.HiddenFor(p => p.ExcessesFpcObj.EiId)
                        <label id="ExcessesFpcObj.Excess-error" class="error" for="ExcessesFpcObj.Excess"></label>
                    </div>
                    <div class="col-md-3">
                        <div class="policy-loc-row">
                            <input type="checkbox" name="ImposedpetsObj.Imposed" id="imposed" class="vlidationInsure"><label for="checkbox1"><span><span></span></span>Imposed</label>
                            <label id="ImposedpetsObj.Impose-error" class="error" for="ImposedpetsObj.Impose"></label>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 pull-right text-right">
                    <button type="button" class="button-3d nomargin tab-linker quot-next-btn homedescrtn" id="ptabbtn1">Next</button>
                </div>
            </div>
            <div id="ptab2">
                <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 policy-full-head"><h2>FARM MACHINERY</h2></div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-6"><p>How much cover do you need for unspecified farm machinery?<br /><span class="mini-text">(You can insure unspecified items of farm machinery up to $5000 for any one item.)</span></p></div>
                    <div class="col-md-6">
                        $@Html.TextBoxFor(p => p.FarmContentFMObj.Farmcontents, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.FarmContentFMObj.EiId, @id = "farmcontents", @style = "display:inline-block;width:95%" })
                        @Html.HiddenFor(p => p.FarmContentFMObj.EiId)
                        <label id="FarmContentFMObj.Farmcontents-error" class="error" for="FarmContentFMObj.Farmcontents"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12 ">
                    <div class="col-md-6"><p>Excess for unspecified machinery </p></div>
                    <div class="col-md-3">
                        @Html.DropDownListFor(Model => Model.ExcessUMObj.Excessum, @Model.ExcessUMObj.ExcessumList, new { @class = "gray-bg-underline vlidationInsure form-control form-border-bottom field-length-sm commondataselection", @data_eiid = @Model.ExcessUMObj.EiId, @id = "excessum" })
                        @Html.HiddenFor(p => p.ExcessUMObj.EiId)
                        <label id="ExcessUMObj.Excessum-error" class="error" for="ExcessUMObj.Excessum"></label>
                    </div>
                    <div class="col-md-3">
                        <div class="policy-loc-row">
                            <input type="checkbox" name="ImposedpetsObj.Imposedum" id="imposedum" class="vlidationInsure"><label for="checkbox1"><span><span></span></span>Imposed</label>
                            <label id="ImposedpetsObj.Imposedum-error" class="error" for="ImposedpetsObj.Imposedum"></label>
                        </div>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <p>Please list your specified item of machinery (valued over $5000 per item) below</p>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-12 table-8">
                        <table class="table" style=" width: 100%; ">
                            <thead>
                                <tr>
                                    <th class="space_left">Description</th>
                                    <th class="space_left">Year</th>
                                    <th class="space_left">Serial Number</th>
                                    <th class="space_left">Excess</th>
                                    <th class="space_left">Sum Insured</th>
                                    <th class="space_right">&nbsp;</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr id="tnameFM-tr-1">
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.DescriptionFmObj.Description, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.DescriptionFmObj.EiId, @data_itid = "0", @id = "description1" })
                                        @Html.HiddenFor(p => p.DescriptionFmObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.YearFMObj.Year, new { @class = "form-control form-border-bottom year vlidationInsure commondatatest", @data_eiid = @Model.YearFMObj.EiId, @data_itid = "0", @id = "year1", @readonly = "readonly" })
                                        @Html.HiddenFor(p => p.YearFMObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.SerialnumberObj.Serialnumber, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.SerialnumberObj.EiId, @data_itid = "0", @id = "serialnumber1" })
                                        @Html.HiddenFor(p => p.SerialnumberObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        <div class="col-md-8 pt-10 pb-10 nopadding">
                                            @Html.DropDownListFor(Model => Model.ExcessesFMObj.Excess, @Model.ExcessesFpcObj.ExcessList, new { @class = "form-control vlidationInsure form-border-bottom commondataselection", @data_eiid = @Model.ExcessesFMObj.EiId, @data_itid = "0", @id = "excess1" })
                                            @Html.HiddenFor(p => p.ExcessesFMObj.EiId)
                                        </div>
                                        <div class="col-md-4 p-0 policy-loc-row">
                                            <input type="checkbox" name="ImposedObj.Imposed" id="imposed1"><label for="checkbox1"><span><span></span></span></label>
                                        </div>
                                    </td>
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.SuminsuredFMObj.Suminsured, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.SuminsuredFMObj.EiId, @data_itid = "0", @id = "suminsured1" })
                                        @Html.HiddenFor(p => p.SuminsuredFMObj.EiId)
                                    </td>
                                    <td class="no-border" id="tnameFM-1"><a href="javascript:void(0)" onclick="tnameAddFM(1)" class="button-3d">+</a></td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td><label id="DescriptionFmObj.Description-error" class="error" for="DescriptionFmObj.Description"></label></td>
                                    <td><label id="YearFMObj.Year-error" class="error" for="YearFMObj.Year"></label></td>
                                    <td><label id="SerialnumberObj.Serialnumber-error" class="error" for="SerialnumberObj.Serialnumber"></label></td>
                                    <td>
                                        <label id="ExcessesFMObj.Excess-error" class="error" for="ExcessesFMObj.Excess"></label>
                                        <label id="ImposedObj.Imposed-error" class="error" for="ImposedObj.Imposed"></label>
                                    </td>
                                    <td><label id="SuminsuredFMObj.Suminsured-error" class="error" for="SuminsuredFMObj.Suminsured"></label></td>
                                    <td>&nbsp;</td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-4"><p>Total for specified items</p></div>
                    <div class="col-md-8"><input type="text" name="totalspaceific" id="totalspaceific" class="form-control  vlidationInsure form-border-bottom"></div>
                </div>
                <div class="col-md-6 pull-right text-right">
                    <a href="javascript:void(0)" id="ptb12" class="button-3d nomargin tab-linker quot-prev-btn" rel="1">Prev</a>
                    <img src="~/images/button-devider.png">
                    <button type="button" class="button-3d nomargin tab-linker quot-next-btn" id="ptabbtn2">Next</button>
                </div>
            </div>
            <div id="ptab3">
                <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 policy-full-head"><h2>LIVESTOCK</h2></div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-12 table-8">
                        <table class="table" style=" width: 100%; ">
                            <thead>
                                <tr>
                                    <th class="space_left">Description</th>
                                    <th class="space_left">Number of Animals</th>
                                    <th class="space_left">Sum Insured per Animal</th>
                                    <th class="space_left">Sum Insured</th>
                                    <th class="space_right">&nbsp;</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr id="tnameLS-tr-1">
                                    <td class="no-border">
                                        @Html.DropDownListFor(Model => Model.DescriptionLSObj.Description, @Model.DescriptionFCObj.DescriptionList, "-- Select --", new { @class = "form-control form-border-bottom vlidationInsure commondataselection", @data_eiid = @Model.DescriptionLSObj.EiId, @data_itid = "0", @id = "description1" })
                                        @Html.HiddenFor(p => p.DescriptionLSObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.NumberanimalObj.Numberanimal, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.NumberanimalObj.EiId, @data_itid = "0", @id = "numberanimal1" })
                                        @Html.HiddenFor(p => p.NumberanimalObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.SuminsuredperObj.Suminsuredper, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.SuminsuredperObj.EiId, @data_itid = "0", @id = "suminsuredper1" })
                                        @Html.HiddenFor(p => p.SuminsuredperObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.SuminsuredLSObj.Suminsured, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.SuminsuredLSObj.EiId, @data_itid = "0", @id = "suminsured1" })
                                        @Html.HiddenFor(p => p.SuminsuredLSObj.EiId)
                                    </td>
                                    <td class="no-border" id="tnameLS-1"><a href="javascript:void(0)" onclick="tnameAddLS(1)" class="button-3d">+</a></td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td><label id="DescriptionLSObj.Description-error" class="error" for="DescriptionLSObj.Description"></label></td>
                                    <td><label id="NumberanimalObj.Numberanimal-error" class="error" for="NumberanimalObj.Numberanimal"></label></td>
                                    <td><label id="SuminsuredperObj.Suminsuredper-error" class="error" for="SuminsuredperObj.Suminsuredper"></label></td>
                                    <td><label id="SuminsuredLSObj.Suminsured-error" class="error" for="SuminsuredLSObj.Suminsured"></label></td>
                                    <td>&nbsp;</td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-4"><p>Total for livestock</p></div>
                    <div class="col-md-8">$<input name="totallivestock" id="totallivestock" class="form-control form-border-bottom" type="text" style="display:inline-block;width:95%"></div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <p class="col-md-8 col-sm-8 col-xs-12">Do you wish to insure your livestock against dog attack?</p>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.DogattackObj.Dogattack, '1', new { @class = "dogattack vlidationInsure commondataradio", @data_eiid = @Model.DogattackObj.EiId, @id = "dogattack1" })

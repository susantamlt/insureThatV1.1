<script type="text/javascript">
    $(document).ready(function () {
        $.validator.addMethod("regex", function (value, element, regexp) {
            if (regexp.constructor != RegExp)
                regexp = new RegExp(regexp);
            else if (regexp.global)
                regexp.lastIndex = 0;
            return this.optional(element) || regexp.test(value);
        }, "Please check your input.");
        var validator = $("form[name='farmContents']").validate({
            ignore: [],
            rules: {
                "SuminsureHCVdObj.Suminsured": {
                    regex: /^[0-9]*$/,
                },
                "ExcessesHCVObj.Excess": {
                    required: function (element) {
                        if ($("#suminsuredHCB").val() != "") {
                            return true;
                        } else {
                            return false;
                        }
                    },
                },
                "ImposedpetsObj.Imposed1": {
                    required: function (element) {
                        if ($("#excessHCB").val() != "") {
                            return true;
                        } else {
                            return false;
                        }
                    },
                },
                "SuminsuredHbcObj.Suminsured": {
                    regex: /^[0-9]*$/,
                },
                "NumberhiveObj.Numberhive": {
                    required: function (element) {
                        if ($("#suminsuredHCB1").val() != "") {
                            return true;
                        } else {
                            return false;
                        }
                    },
                    regex: /^[0-9]*$/,
                },
                "ExcessBObj.Excess": {
                    required: function (element) {
                        if ($("#suminsuredHCB1").val() != "") {
                            return true;
                        } else {
                            return false;
                        }
                    },
                },
                "ImposedpetsObj.Imposed": {
                    required: function (element) {
                        if ($("#excessbHBC").val() != "") {
                            return true;
                        } else {
                            return false;
                        }
                    },
                },
                "rlsFSFChome": "required",
                "rlsFMhome": "required",
                "rlsLShome": "required",
            },
            messages: {
                "SuminsureHCVdObj.Suminsured": {
                    regex: "Only numeric allowed in sum insured for harvested crops"
                },
                "ExcessesHCVObj.Excess": {
                    required: "Please select excess for harvested crops"
                },
                "ImposedpetsObj.Imposed1": {
                    required: "Please select imposed for harvested crops"
                },
                "SuminsuredHbcObj.Suminsured": {
                    regex: "Only numeric allowed in sum insured for Beehives"
                },
                "NumberhiveObj.Numberhive": {
                    required: "Please enter number of hives",
                    regex: "Only numeric allowed in number of hives"
                },
                "ExcessBObj.Excess": {
                    required: "Please select excess for Beehives"
                },
                "ImposedpetsObj.Imposed": {
                    required: "Please select imposed for Beehives"
                },
                "rlsFSFChome": "Please fillup the farm structures and farm contents,",
                "rlsFMhome": "Please fillup the farm machinery,",
                "rlsLShome": "Please fillup the live stock,",
            },
            onfocusout: function (element) {
                this.element(element);
            },
            submitHandler: function (form) {
                form.submit();
            }
        });
    });
</script>
    @using (Ajax.BeginForm("Valuables", "FarmPolicyValuables", new { cid = @ViewBag.cid }, new AjaxOptions()
    {
        HttpMethod = "POST"

    }, new { @id = "valuablesFP", @name = "valuablesFP" }))

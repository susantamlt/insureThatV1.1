                <label id="CoverUnspecifiedValuablesObj.CoverUnspecifiedValuables-error" class="error" for="CoverUnspecifiedValuablesObj.CoverUnspecifiedValuables"></label>
            </div>
        </div>
        <div class="col-md-12 mt-10 mb-10" id="processTabs">
            <div class="col-md-12 table-8">
                <h5>If you wish to specify any items that are valued greater than $10,000 per item please describe them here</h5>
                <table class="table VTable" style=" width: 100%; ">
                    <thead>
                        <tr>
                            <th class="space_left">Description</th>
                            <th class="space_left">Sum Insured</th>
                            <th class="space_right">&nbsp;</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.SpecifiedItemDescriptionList != null && Model.SpecifiedItemDescriptionList.Count() > 0)
                        {
                            CountTotal = Model.SpecifiedItemDescriptionList.Count();
                            for (int i = 0; i < Model.SpecifiedItemDescriptionList.Count(); i++)
                            {
                                jCount = i + 1;
                                <tr id="tname-tr-@jCount">
                                    @if (jCount == CountTotal)
                                    {
                                        <td class="no-border">
			                                @Html.TextBoxFor(p => p.SpecifiedItemDescriptionObj.ItemDescription, @Model.SpecifiedItemDescriptionList[i].Value, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @id = "description", @data_eiid = @Model.SpecifiedItemDescriptionObj.EiId, @data_regex = "AN", @data_itid = jCount, @autocomplete = "off" })
			                                @Html.HiddenFor(p => p.SpecifiedItemDescriptionObj.EiId)
			                            </td>
			                            <td class="price stotal no-border">
			                                $@Html.TextBoxFor(p => p.SpecifiedItemSumInsuredObj.ItemSumInsured, @Model.SpecifiedItemSumInsuredList[i].Value, new { @style = "display:inline-block;width:95%;", @class = "form-control form-border-bottom autocalculateFHC vlidationInsure commondatatestComma commondatatestPrice", @id = "suminsured", @data_eiid = @Model.SpecifiedItemSumInsuredObj.EiId, @data_regex = 'N', @data_itid = jCount, @autocomplete = "off" })
			                                @Html.HiddenFor(p => p.SpecifiedItemSumInsuredObj.EiId)
			                            </td>
			                            <td class="no-border" id="tname-@jCount"><a href="javascript:void(0)" onclick="tnameAdd(@jCount)" class="button-3d">+</a></td>
                                    }
                                    else
                                    {
                                        <td class="no-border">
			                                @Html.TextBoxFor(p => p.SpecifiedItemDescriptionObj.ItemDescription, @Model.SpecifiedItemDescriptionList[i].Value, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @id = "description", @data_eiid = @Model.SpecifiedItemDescriptionObj.EiId, @data_regex = "AN", @data_itid = jCount, @autocomplete = "off" })
			                                @Html.HiddenFor(p => p.SpecifiedItemDescriptionObj.EiId)
			                            </td>
			                            <td class="price stotal no-border">
			                                $@Html.TextBoxFor(p => p.SpecifiedItemSumInsuredObj.ItemSumInsured, @Model.SpecifiedItemSumInsuredList[i].Value, new { @style = "display:inline-block;width:95%;", @class = "form-control form-border-bottom autocalculateFHC vlidationInsure commondatatestComma commondatatestPrice", @id = "suminsured", @data_eiid = @Model.SpecifiedItemSumInsuredObj.EiId, @data_regex = 'N', @data_itid = jCount, @autocomplete = "off" })
			                                @Html.HiddenFor(p => p.SpecifiedItemSumInsuredObj.EiId)
			                            </td>
			                            <td class="no-border" id="tname-@jCount"><a href="javascript:void(0)" onclick="tnameRemove(@jCount)" class="button-3d">-</a></td>
                                    }
                                </tr>
                            }
                        }
                        else
                        {
                            <tr id="tname-tr-1">
	                            <td class="no-border">
	                                @Html.TextBoxFor(p => p.SpecifiedItemDescriptionObj.ItemDescription, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @id = "description", @data_eiid = @Model.SpecifiedItemDescriptionObj.EiId, @data_regex = "AN", @data_itid = "0", @autocomplete = "off" })
	                                @Html.HiddenFor(p => p.SpecifiedItemDescriptionObj.EiId)
	                            </td>
	                            <td class="price stotal no-border">
	                                $@Html.TextBoxFor(p => p.SpecifiedItemSumInsuredObj.ItemSumInsured, new { @style = "display:inline-block;width:95%;", @class = "form-control form-border-bottom autocalculateFHC vlidationInsure commondatatestComma commondatatestPrice", @id = "suminsured", @data_eiid = @Model.SpecifiedItemSumInsuredObj.EiId, @data_regex = 'N', @data_itid = "0", @autocomplete = "off" })
	                                @Html.HiddenFor(p => p.SpecifiedItemSumInsuredObj.EiId)
	                            </td>
	                            <td class="no-border" id="tname-1"><a href="javascript:void(0)" onclick="tnameAdd(1)" class="button-3d">+</a></td>
	                        </tr>
                        }
                    </tbody>
                    <tfoot>
                        <tr>
                            <td class="no-border"><label id="SpecifiedItemDescriptionObj.ItemDescription-error" class="error" for="SpecifiedItemDescriptionObj.ItemDescription"></label></td>
                            <td class="no-border"><label id="SpecifiedItemSumInsuredObj.ItemSumInsured-error" class="error" for="SpecifiedItemSumInsuredObj.ItemSumInsured"></label></td>
                            <td class="no-border">&nbsp;</td>
                        </tr>
                        <tr>
                            <td class="no-border"><p class="form-control form-border-bottom" style="margin-top: 0px;">Total value</p></td>
                            <td class="no-border">$<input type="text" style="width:95%; display:inline-block;" readonly="readonly" name="totalSumInsured" id="totalSumInsured" class="form-control form-border-bottom" placeholder="Auto calculate"></td>
                            <td class="no-border">&nbsp;</td>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <p class="col-md-6 col-sm-6 col-xs-12">Please select <a href="#ExcesshomeBuildingModel" role="button" data-target="#ExcesshomeBuildingModel" data-toggle="modal">Excess</a></p>
            <div class="col-md-3">
                @Html.DropDownListFor(p => p.ExcessValuablesObj.Excess, listsExcess, new { @class = "form-control form-border-bottom commondataselection vlidationInsure", @id = "excess", @data_eiid = @Model.ExcessValuablesObj.EiId })
                @Html.HiddenFor(p => p.ExcessValuablesObj.EiId)
                <label id="ExcessValuablesObj.Excess-error" class="error" for="ExcessValuablesObj.Excess"></label>
            </div>
            @*<div class="col-md-3">
                    <div class="policy-loc-row">
                        <input type="checkbox" name="ImposedvalObj.Imposed" id="imposed"><label for="checkbox1"><span><span></span></span>Imposed</label>
                        <label id="imposed-error" class="error" for="imposed"></label>
                    </div>
                </div>*@
        </div>

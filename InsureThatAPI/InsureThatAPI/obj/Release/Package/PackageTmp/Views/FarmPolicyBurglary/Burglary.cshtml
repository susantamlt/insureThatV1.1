@model InsureThatAPI.Models.FPBurglary
@{
    ViewBag.Title = "Burglary";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<SelectListItem> listsExcess = new List<SelectListItem>();
    listsExcess.Add(new SelectListItem { Value = "100", Text = "$100" });
    listsExcess.Add(new SelectListItem { Value = "200", Text = "$200" });
    listsExcess.Add(new SelectListItem { Value = "500", Text = "$500" });
    listsExcess.Add(new SelectListItem { Value = "1000", Text = "$1,000" });
    listsExcess.Add(new SelectListItem { Value = "2000", Text = "$2,000" });
}
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 policy-indiv-head">
    <h4><span>FARM POLICY</span> / BURGLARY</h4>
</div>
<div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 endorsmentview">
    <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 policy-full-head">
        <h2>Burglary</h2>
    </div>
    <div class="col-md-5 col-sm-5 col-xs-12 pull-right">
        <div class="pull-right">
            <button type="button" href="#savepolicy" role="button" data-target="#savepolicy" data-toggle="modal" class="button-3d nomargin savepolicyP tab-linker" id="savepolicyP" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Save Policy</button>
            <button type="button" href="#referralListModel" role="button" data-target="#referralListModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="referralList" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Referral List</button>
            <button type="button" href="#premiumModel" role="button" data-target="#premiumModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="premium" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Premium</button>
        </div>
    </div>
    <div id="savepolicy" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="savepolicy" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Save Policy</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <p>Do you wish to save the answers you have completed and continue later?</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                    <button class="btn btn-default no-border savepolicyP" data-dismiss="modal" aria-hidden="true"> Save </button>
                </div>
            </div>
        </div>
    </div>
    <div id="referralListModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="referralListModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="pull-left">Referral List</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    @if (Model.Referels != null && Model.Referels.Count() > 0)
                    {
                        for (int j = 0; j < Model.Referels.Count(); j++)
                        {
                            @Model.Referels[j]<br />
                        }
                    }
                    else
                    {
                        <p>No Referral List</p>
                    }
                    <p id="referralList-p"></p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div id="premiumModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="premiumModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Premium Details</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table" style=" width: 100%; ">
                        <thead>
                            <tr>
                                <th class="space_left">Section Name</th>
                                <th class="space_left">Base</th>
                                <th class="space_left">FSL</th>
                                <th class="space_left">GST</th>
                                <th class="space_left">Stamp Duty</th>
                                <th class="space_right">Gross</th>
                            </tr>
                        </thead>
                        <tbody id="premiumModel-p-b">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tbody>
                        <tfoot id="premiumModel-p-f">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 success" id="savepolicyresult"></div>
    @using (Ajax.BeginForm("Burglary", "FarmPolicyBurglary", new { cid = @ViewBag.cid }, new AjaxOptions()
    {
        HttpMethod = "POST"
    }, new { @id = "burglary", @name = "burglary" }))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(p => p.CustomerId, new { @id = "ciid" })
        @Html.HiddenFor(p => p.PcId, new { id = "pcidd" })
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <h5 class="col-md-12"><a href="#burglaryCoverModal" role="button" data-target="#burglaryCoverModal" data-toggle="modal">Click here for the definition of Burglary.</a></h5>
            <div class="col-md-4"><p>Location</p></div>
            <div class="col-md-8">
                <div class="col-md-10 col-sm-10 col-xs-10">
                    @Html.TextBoxFor(p => p.AddressObj.Address, new { @class = "form-control form-border-bottom vlidationInsure", @id = "addressBug", @data_eiid = @Model.AddressObj.EiId, @readonly = "readonly" })
                    @Html.HiddenFor(p => p.AddressObj.EiId)
                    <label id="AddressObj.Address-error" class="error" for="AddressObj.Address"></label>
                </div>
            </div>
        </div>
        <h5 class="col-md-12 mt-10">Cover Option</h5>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <p class="col-md-6 col-sm-6 col-xs-12">Do you wish to cover your property against loss or accidental damage caused by <a href="#burglaryCoverModal" role="button" data-target="#burglaryCoverModal" data-toggle="modal">Burglary?</a></p>
            <div class="col-md-4 col-sm-4 col-xs-12">
                <div class="policy-loc-row">
                    @if (Model.CoverYourPropOptionFPObj.Cover == "1")
                    {
                         <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.CoverYourPropOptionFPObj.Cover, '1', new { @class = "buildingtype vlidationInsure commondataradio", @id = "buildingtype1", @checked = "checked", @data_eiid = @Model.CoverYourPropOptionFPObj.EiId })<label for="radio1"><span><span></span></span>Yes</label>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.CoverYourPropOptionFPObj.Cover, '0', new { @class = "buildingtype vlidationInsure commondataradio", @id = "buildingtype2",@data_eiid = @Model.CoverYourPropOptionFPObj.EiId })<label for="radio1"><span><span></span></span>No</label>
                        </div>
                    }
                    else
                    {
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.CoverYourPropOptionFPObj.Cover, '1', new { @class = "buildingtype vlidationInsure commondataradio", @id = "buildingtype1", @data_eiid = @Model.CoverYourPropOptionFPObj.EiId })<label for="radio1"><span><span></span></span>Yes</label>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.CoverYourPropOptionFPObj.Cover, '0', new { @class = "buildingtype vlidationInsure commondataradio", @id = "buildingtype2", @checked = "checked", @data_eiid = @Model.CoverYourPropOptionFPObj.EiId })<label for="radio1"><span><span></span></span>No</label>
                        </div>
                    }
                    @Html.HiddenFor(p => p.CoverYourPropOptionFPObj.EiId)
                </div>
                <label id="CoverYourPropOptionFPObj.Cover-error" class="error" for="CoverYourPropOptionFPObj.Cover"></label>
            </div>
        </div>
        <p class="col-md-12 mb-md-30 mt-10 mb-10 mini-text">If you select Burglary cover, the amounts you have selected under Fixed Farm Property and Mobile Farm Property are incorporated into the burglary section.   </p>
        <h5 class=" mt-10 col-md-12">Details of Cover</h5>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Farm Buildings (excluding coolrooms)</p></div>
            <div class="col-md-6">
                $@Html.TextBoxFor(p => p.FarmBuildingExCoolRoomsFPObj.FarmBuildingExcCoolRooms, new { @style = "display: inline-block;width: 97%;", @class = "form-control form-border-bottom commondatatestComma commondatatestPrice vlidationInsure", @id = "CoverFB", @data_eiid = @Model.FarmBuildingExCoolRoomsFPObj.EiId, @data_regex = "AN", @data_itid = "0" })
                @Html.HiddenFor(p => p.FarmBuildingExCoolRoomsFPObj.EiId)
                <label id="FarmBuildingExCoolRoomsFPObj.FarmBuildingExcCoolRooms-error" class="error" for="FarmBuildingExCoolRoomsFPObj.FarmBuildingExcCoolRooms"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Coolrooms</p></div>
            <div class="col-md-6">
                $@Html.TextBoxFor(p => p.CoolRoomsFPObj.CoolRooms, new { @style = "display: inline-block;width: 97%;", @class = "form-control form-border-bottom commondatatestComma commondatatestPrice vlidationInsure", @id = "FPCoolRooms", @data_eiid = @Model.CoolRoomsFPObj.EiId, @data_regex = "N", @data_itid = "0" })
                @Html.HiddenFor(p => p.CoolRoomsFPObj.EiId)
                <label id="CoolRoomsFPObj.CoolRooms-error" class="error" for="CoolRoomsFPObj.CoolRooms"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Farm Fencing</p></div>
            <div class="col-md-6">
                $@Html.TextBoxFor(p => p.FarmFencingFPObj.FarmFencing, new { @style = "display: inline-block;width: 97%;", @class = "form-control form-border-bottom commondatatestComma commondatatestPrice vlidationInsure", @id = "FPFarmFencing", @data_eiid = @Model.FarmFencingFPObj.EiId, @data_regex = "AN", @data_itid = "0" })
                @Html.HiddenFor(p => p.FarmFencingFPObj.EiId)
                <label id="FarmFencingFPObj.FarmFencing-error" class="error" for="FarmFencingFPObj.FarmFencing"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Other Farm Structures</p></div>
            <div class="col-md-6 col-sm-6 col-xs-12">
                $@Html.TextBoxFor(p => p.OtherFarmStructuresFPObj.OtherFarmStructures, new { @style = "display: inline-block;width: 97%;", @class = "form-control form-border-bottom commondatatestComma commondatatestPrice vlidationInsure", @id = "FPOtherFarm", @data_eiid = @Model.OtherFarmStructuresFPObj.EiId, @data_regex = "AN", @data_itid = "0" })
                @Html.HiddenFor(p => p.OtherFarmStructuresFPObj.EiId)
                <label id="OtherFarmStructuresFPObj.OtherFarmStructures-error" class="error" for="OtherFarmStructuresFPObj.OtherFarmStructures"></label>
                <p class="mini-text">(Includes covered ways, exterior light fittings, irrigation systems, pens, sheep dips, stockyards, pipes, cables, power and telephone poles, silos, solar power units, tanks and windmills.)</p>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Farm Contents</p></div>
            <div class="col-md-6">
                $@Html.TextBoxFor(p => p.FarmContentsFPObj.FarmContents, new { @style = "display: inline-block;width: 97%;", @class = "form-control form-border-bottom commondatatestComma commondatatestPrice vlidationInsure", @id = "FPFarmContents", @data_eiid = @Model.FarmContentsFPObj.EiId, @data_regex = "AN", @data_itid = "0" })
                @Html.HiddenFor(p => p.FarmContentsFPObj.EiId)
                <label id="FarmContentsFPObj.FarmContents-error" class="error" for="FarmContentsFPObj.FarmContents"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Hail netting stored in a Farm Structure</p></div>
            <div class="col-md-6 col-sm-6 col-xs-12">
                $@Html.TextBoxFor(p => p.HailNettingStoredFPObj.HailNettingStored, new { @style = "display: inline-block;width: 97%;", @class = "form-control form-border-bottom commondatatestComma commondatatestPrice vlidationInsure", @id = "FPHailNetting", @data_eiid = @Model.HailNettingStoredFPObj.EiId, @data_regex = "AN", @data_itid = "0" })
                @Html.HiddenFor(p => p.HailNettingStoredFPObj.EiId)
                <label id="HailNettingStoredFPObj.HailNettingStored-error" class="error" for="HailNettingStoredFPObj.HailNettingStored"></label>
                <p class="mini-text">with roof and walls (fire only)</p>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Unspecified machinery</p></div>
            <div class="col-md-6 col-sm-6 col-xs-12">
                $@Html.TextBoxFor(p => p.UnspecifiedMachineryFPObj.UnspecifiedMachinery, new { @style = "width:95%; display:inline-block;", @class = "form-control form-border-bottom vlidationInsure commondatatestComma commondatatestPrice", @id = "FPUnspecMach", @data_eiid = @Model.UnspecifiedMachineryFPObj.EiId, @data_regex = 'N', @data_itid = "0" })
                @Html.HiddenFor(p => p.UnspecifiedMachineryFPObj.EiId)
                <label id="UnspecifiedMachineryFPObj.UnspecifiedMachinery-error" class="error" for="UnspecifiedMachineryFPObj.UnspecifiedMachinery"></label>
                <p class="mini-text">limit $5,000 per item</p>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Specified Items over $5,000 per item</p></div>
            <div class="col-md-6 col-sm-6 col-xs-12">
                $@Html.TextBoxFor(p => p.SpecifiedItemsOver5KFPObj.SpecifiedItemOver5K, new { @style = "width:95%;display:inline-block", @class = "form-control form-border-bottom vlidationInsure commondatatestComma commondatatestPrice", @id = "FPSpecItems", @data_eiid = @Model.SpecifiedItemsOver5KFPObj.EiId, @data_regex = 'N', @data_itid = "0" })
                @Html.HiddenFor(p => p.SpecifiedItemsOver5KFPObj.EiId)
                <label id="SpecifiedItemsOver5KFPObj.SpecifiedItemOver5K-error" class="error" for="SpecifiedItemsOver5KFPObj.SpecifiedItemOver5K"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Please select <a href="#ExcesshomeBuildingModel" role="button" data-target="#ExcesshomeBuildingModel" data-toggle="modal">Excess</a> for burglary</p></div>
            <div class="col-md-3">
                @Html.DropDownListFor(p => p.ExcessFPBurglaryObj.Excess, listsExcess, new { @class = "gray-bg-underline form-control form-border-bottom field-length-sm commondataselection vlidationInsure", @id = "excess", @data_eiid = @Model.ExcessFPBurglaryObj.EiId })
                @Html.HiddenFor(p => p.ExcessFPBurglaryObj.EiId)
                <label id="ExcessFPBurglaryObj.Excess-error" class="error" for="ExcessFPBurglaryObj.Excess"></label>
            </div>
            @*<div class="col-md-3">
                <div class="policy-loc-row">
                    <input type="checkbox" name="ImposedBuglaryObj.Imposed" id="imposed"><label for="checkbox1"><span><span></span></span>Imposed</label>
                    <label id="ImposedBuglaryObj.Imposed-error" class="error" for="ImposedBuglaryObj.Imposed"></label>
                </div>
            </div>*@
        </div>
        <div id="ExcesshomeBuildingModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="ExcesshomeBuildingModel" aria-hidden="false">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h3 class="pull-left">Excess</h3>
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            <img src="~/images/close-icon.png" />
                        </button>
                    </div>
                    <div class="modal-body">
                        <p>When you make a claim, you pay the first part of the cost and we pay the rest. Your part is called the <b>excess</b>.  You can reduce the cost of your insurance by choosing a higher excess.</p>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                    </div>
                </div>
            </div>
        </div>
        <div id="theftModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="theftModel" aria-hidden="false">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h3 class="pull-left">Theft</h3>
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            <img src="~/images/close-icon.png" />
                        </button>
                    </div>
                    <div class="modal-body">
                        <p>(1) Theft from You that is not accompanied by intimidation or violence to a person; or</p>
                        <p>(2) Theft without violent and forceful entry into or exit from a building</p>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                    </div>
                </div>
            </div>
        </div>
        <h5 class="col-md-12 mt-10">Optional Extras</h5>
        <p class="col-md-12">You can extend this cover to include cover by <a href="#theftModel" role="button" data-target="#theftModel" data-toggle="modal">Theft</a> (without forcible or violent entry) at each listed location and in transit between all the locations you have listed on your policy.</p>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <p class="col-md-6 col-sm-6 col-xs-12">Do you wish to cover theft of your farm structures and <a href="#burglarycontentsModal" role="button" data-target="#burglarycontentsModal" data-toggle="modal">Farm Contents?</a></p>
            <div class="col-md-4 col-sm-4 col-xs-12">
                <div class="policy-loc-row">
                    @if (Model.OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC == "1")
                    {
                         <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC, '1', new { @class = "buildingtype vlidationInsure commondataradio", @id = "CoverTheftFSandFC1", @checked = "checked", @data_eiid = @Model.OptFPCoverTheftFSAndFCObj.EiId })<label for="radio1"><span><span></span></span>Yes</label>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC, '0', new { @class = "buildingtype vlidationInsure commondataradio", @id = "CoverTheftFSandFC2",@data_eiid = @Model.OptFPCoverTheftFSAndFCObj.EiId })<label for="radio1"><span><span></span></span>No</label>
                        </div>
                    }
                    else
                    {
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC, '1', new { @class = "buildingtype vlidationInsure commondataradio", @id = "CoverTheftFSandFC1", @data_eiid = @Model.OptFPCoverTheftFSAndFCObj.EiId })<label for="radio1"><span><span></span></span>Yes</label>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC, '0', new { @class = "buildingtype vlidationInsure commondataradio", @id = "CoverTheftFSandFC2", @checked = "checked", @data_eiid = @Model.OptFPCoverTheftFSAndFCObj.EiId })<label for="radio1"><span><span></span></span>No</label>
                        </div>
                    }
                    @Html.HiddenFor(p => p.OptFPCoverTheftFSAndFCObj.EiId)
                </div>
                <label id="OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC-error" class="error" for="OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <p class="col-md-6 col-sm-6 col-xs-12">Do you wish to cover <a href="#theftModel" role="button" data-target="#theftModel" data-toggle="modal">Theft</a> of your Farm Machinery?</p>
            <div class="col-md-4 col-sm-4 col-xs-12">
                <div class="policy-loc-row">
                    @if (Model.OptFPCoverTheftFarmMachineryObj.CoverTheftFarmMachinery == "1")
                    {
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.OptFPCoverTheftFarmMachineryObj.CoverTheftFarmMachinery, '1', new { @class = "buildingtype vlidationInsure commondataradio", @id = "CoverTheftFarmMachinery1", @checked = "checked", @data_eiid = @Model.OptFPCoverTheftFarmMachineryObj.EiId })<label for="radio1"><span><span></span></span>Yes</label>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                            @Html.RadioButtonFor(m => m.OptFPCoverTheftFarmMachineryObj.CoverTheftFarmMachinery, '0', new { @class = "buildingtype vlidationInsure commondataradio", @id = "CoverTheftFarmMachinery2",@data_eiid = @Model.OptFPCoverTheftFarmMachineryObj.EiId })<label for="radio1"><span><span></span></span>No</label>
                        </div>
                    }
                    else
                    {
                        <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">

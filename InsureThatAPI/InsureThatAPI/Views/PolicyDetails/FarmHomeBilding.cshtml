                                                {
                                                    if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62089).Value == "1")
                                                    {
                                                        <span>Cement tiles</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62089).Value == "2")
                                                    {
                                                        <span>Colourbond/zincalume</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62089).Value == "3")
                                                    {
                                                        <span>Corrugated Iron* (if selected a further question to ask if roof has any rust?</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62089).Value == "4")

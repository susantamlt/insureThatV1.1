                                                    {
                                                        <span>Brick Veneer</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62085).Value == "3")
                                                    {
                                                        <span>Cladding *</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62085).Value == "4")
                                                    {
                                                        <span>Concrete</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62085).Value == "5")
                                                    {
                                                        <span>Concrete Block</span>
                                                    }

                                            <p class="col-md-8 col-sm-8 col-xs-12">Address</p>
                                            <p class="col-md-4 col-sm-4 col-xs-12 text-right">
                                                @if (Model.AddressData != null && Model.AddressData.Count > 0)
                                                {
                                                    <span>@Model.AddressData[0].AddressLine1,&nbsp;@Model.AddressData[0].Suburb,&nbsp;@Model.AddressData[0].State,&nbsp;@Model.AddressData[0].Postcode</span>
                                                }
                                            </p>
                                        </div>
                                        <div class="row">
                                            <p class="col-md-10 col-sm-10 col-xs-10">Is your Home Building within 100 metres from Uncleared Natural Bushland, a State or National Forest or a Commercial Plantation of pine trees?</p>
                                            <p class="col-md-2 col-sm-2 col-xs-2 text-right">
                                                @if (Model.ProfileData.ValueData.Exists(p => p.Element.ElId == 62067) && (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62067).Value != null))
                                                {
                                                    if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62067).Value == "1")
                                                    {
                                                        <span>Yes</span>
                                                    }
                                                    else
                                                    {
                                                        <span>No</span>
                                                    }
                                                }
                                            </p>
                                        </div>
                                        <div class="row">
                                            <p class="col-md-12 col-sm-12 col-xs-12 alert alert-info"><strong>Construction Details</strong></p>
                                        </div>
                                        <div class="row">
                                            <p class="col-md-10 col-sm-10 col-xs-10">What are the external walls made of?</p>
                                            <p class="col-md-2 col-sm-2 col-xs-2 text-right">
                                                @if (Model.ProfileData.ValueData.Exists(p => p.Element.ElId == 62085) && (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62085).Value != null))
                                                {

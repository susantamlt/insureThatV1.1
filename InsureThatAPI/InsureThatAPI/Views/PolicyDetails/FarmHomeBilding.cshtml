                                                    {
                                                        <span>Double Brick</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62085).Value == "7")
                                                    {
                                                        <span>Metel</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62085).Value == "8")
                                                    {
                                                        <span>Mud Brick</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62085).Value == "9")
                                                    {
                                                        <span>Other*</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62085).Value == "10")
                                                    {
                                                        <span>Rock/Stone</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62085).Value == "11")
                                                    {
                                                        <span>Rockcote</span>
                                                    }

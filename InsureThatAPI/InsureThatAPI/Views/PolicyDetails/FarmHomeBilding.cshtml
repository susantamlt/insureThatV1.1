                                                    {
                                                        <span>Metel Tiles</span>
                                                    }
                                                    else if (Model.ProfileData.ValueData.Find(p => p.Element.ElId == 62089).Value == "6")
                                                    {
                                                        <span>Other*</span>
                                                    }

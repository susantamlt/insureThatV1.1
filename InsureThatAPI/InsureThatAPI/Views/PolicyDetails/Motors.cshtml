                                        if (Model.SectionData.ValueData[i].Element.ElId == 60969 && Model.SectionData.ValueData[i].Value != "")
                                        {
                                            <span>@Model.SectionData.ValueData[i].Value</span><br /><br />
                                        }
                                    }
                                }
                            </p>
                        </div>
                        <div class="row">
                            <button type="submit" class="btn-primary btn pull-right" style="margin-top:5% !important;">NEXT</button>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
}
else
{

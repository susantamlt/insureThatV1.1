@model InsureThatAPI.Models.MobileFarmContents
@{
    ViewBag.Title = "Farm Contents";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<SelectListItem> listsExcess = new List<SelectListItem>();
    listsExcess.Add(new SelectListItem { Value = "250", Text = "$250" });
    listsExcess.Add(new SelectListItem { Value = "500", Text = "$500" });
    listsExcess.Add(new SelectListItem { Value = "1000", Text = "$1,000" });
    listsExcess.Add(new SelectListItem { Value = "2000", Text = "$2,000" });
    List<SelectListItem> listsExcessFM = new List<SelectListItem>();
    listsExcessFM.Add(new SelectListItem { Value = "500", Text = "$500" });
    listsExcessFM.Add(new SelectListItem { Value = "750", Text = "$750" });
    listsExcessFM.Add(new SelectListItem { Value = "1250", Text = "$1,250" });
    listsExcessFM.Add(new SelectListItem { Value = "2250", Text = "$2,250" });
    var CountTotal = 0;
    var jCount = 0;
}
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 policy-indiv-head">
    <h4><span>FARM POLICY</span> / MOBILE FARM PROPERTY</h4>
</div>
<div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 endorsmentview">
    <div class="col-md-5 col-sm-5 col-xs-12 pull-right">
        <div class="pull-right">
            <button type="button" href="#savepolicy" role="button" data-target="#savepolicy" data-toggle="modal" class="button-3d nomargin tab-linker" id="premium" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Save Policy</button>
            <button type="button" href="#referralListModel" role="button" data-target="#referralListModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="referralList" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Referral List</button>
            <button type="button" href="#premiumModel" role="button" data-target="#premiumModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="premium" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Premium</button>
        </div>
    </div>
    <div id="savepolicy" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="savepolicy" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Save Policy</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <p>Do you wish to save the answers you have completed and continue later?</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                    <button class="btn btn-default no-border savepolicyP" data-dismiss="modal" aria-hidden="true"> Save </button>
                </div>
            </div>
        </div>
    </div>
    <div id="referralListModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="referralListModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="pull-left">Referral List</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    @if (Model.Referels != null && Model.Referels.Count() > 0)
                    {
                        for (int j = 0; j < Model.Referels.Count(); j++)
                        {
                            @Model.Referels[j]<br />
                        }
                    }
                    else
                    {
                        <p>No Referral List</p>
                    }
                    <p id="referralList-p"></p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div id="premiumModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="premiumModel" aria-hidden="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Premium Details</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table" style=" width: 100%; ">
                        <thead>
                            <tr>
                                <th class="space_left">Section Name</th>
                                <th class="space_left">Base</th>
                                <th class="space_left">FSL</th>
                                <th class="space_left">GST</th>
                                <th class="space_left">Stamp Duty</th>
                                <th class="space_right">Gross</th>
                            </tr>
                        </thead>
                        <tbody id="premiumModel-p-b">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tbody>
                        <tfoot id="premiumModel-p-f">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div class="mt-40 mb-40 row"></div>
    <div class="clearfix"></div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 success" id="savepolicyresult"></div>
    <div id="processTabs">
        <ul class="process-steps clearfix col-sm-12">
            <li><a href="javascript:void(0)" id="ptb1">Farm Contents</a></li>
            <li><a href="javascript:void(0)" id="ptb2">Farm Machinery</a></li>
            <li><a href="javascript:void(0)" id="ptb3">Livestock</a></li>
            <li><a href="javascript:void(0)" id="ptb4">Working Dogs and Beehives</a></li>
        </ul>
        <div>
            <div id="errorContainer"></div>
            <div class="errors"></div>
            @using (Ajax.BeginForm("FarmContents", "MobileFarm", new { cid = @ViewBag.cid }, new AjaxOptions()
            {
                HttpMethod = "POST"

            }, new { @id = "farmContents", @name = "farmContents" }))
            {
                @Html.AntiForgeryToken()
                @Html.HiddenFor(p => p.CustomerId, new { @id = "ciid" })
                @Html.HiddenFor(p => p.PcId, new { id = "pcidd" })
                <div id="ptab1">
                    <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 policy-full-head"><h2>FARM CONTENTS</h2></div>
                    <div class="col-md-12">
                        <h5><a href="#costumModal1" role="button" data-target="#costumModal1" data-toggle="modal"><span class="cover-options">Click here for Farm Contents definition</span></a></h5>
                        <div id="costumModal1" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h3 class="pull-left">Farm Contents</h3>
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                            <img src="~/images/close-icon.png" />
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <ul style="list-style:none">
                                            <li>(1) All farm contents on Your farm; and</li>
                                            <li>(2) Any office equipment in Your Home Buildings.</li>
                                            <li>(3) Portable pens, sheep dips and stockyards;</li>
                                            <li>(4) Portable silos;</li>
                                            <li>(5) Portable solar power units</li>
                                            <li>(6) Portable farm fencing.</li>
                                        </ul>
                                        <p>It does not mean any:</p>
                                        <ul style="list-style:none">
                                            <li>(1) Item that can be more specifically insured under another section of this policy;</li>
                                            <li>(2) Home Contents;</li>
                                            <li>(3) Valuables;</li>
                                            <li>(4) Mobile phone, laptop computer, hand held computer or global positioning system unless they are used for or connected to Your business as specified in the Certificate;</li>
                                            <li>(5) Aircraft; balloon; drone; craft that travels on a cushion of air; rocket or an accessory to such items;</li>
                                            <li>(6) Business book, document, record or plan except for the material on which they are written or stored. We do cover blank stationery.</li>
                                            <li>(7) Cash, cheque, postal note, money order, stamp, credit card voucher or negotiable instrument;</li>
                                            <li>(8) Jewellery, fur, precious metal or stone;</li>
                                            <li>(9) Living creature;</li>
                                            <li>(10) Farm Machinery nor self-propelled or motor drawn machine or implement not designed specifically for agricultural purposes or used primarily for agricultural purposes;</li>
                                            <li>(11) Harvested crops;</li>
                                            <li>(12) Animal semen or embryos, and their containers (cover available under the Livestock section);</li>
                                            <li>(13) Beehives (cover available under Beehives in the Mobile Farm Property section);</li>
                                            <li>(14) Motor vehicle, motorcycle, motor scooter or an accessory to such items;</li>
                                            <li>(15) Property that is described under Farm Structures;</li>
                                            <li>(16) Vehicle that runs on rails or an accessory to such a vehicle;</li>
                                            <li>(17) Watercraft or an accessory to a watercraft.</li>
                                        </ul>
                                    </div>
                                    <div class="modal-footer">
                                        <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix mb-md-30 mt-10 mb-10" id="locationCovered-1">
                        <div class="col-md-3"><p>Location covered</p></div>
                        <div class="col-md-9">
                            <div class="col-md-11" style="padding-left:0px;padding-right:0px;">
                                @*<input type="text" name="AdaddressObj.Address" data-regex="ANS" id="Address" readonly="readonly" class="form-control form-border-bottom">*@
                                    <div class="col-md-6" style="padding-left:0px;">
                                        @Html.TextBoxFor(p => p.AddressObj.Address, new { @class = "form-control form-border-bottom vlidationInsure",@readonly="readonly", @placeholder = "Address", @id = "addresshome", @style = "width:100%", @autocomplete = "off" })
                                        <label id="AddressObj.Address-error" class="error" for="AddressObj.Address"></label>
                                    </div>
                                    <div class="col-md-6" style="padding:0px;">
                                        @Html.TextBoxFor(p => p.LocationObj.Location, new { @class = "form-control form-border-bottom vlidationInsure", @readonly = "readonly", @placeholder = "Suburb", @id = "suburb", @name = "SubUrb", @style = "width:100%" })
                                        <label id="LocationObj.Location-error" class="error" for="LocationObj.Location"></label>
                                    </div>
                                <label id="AdaddressObj.Address-error" class="error" for="AdaddressObj.Address"></label>
                            </div>
                            @*<div class="col-md-1">
                                <button type="button" id="openNewAddressSet" class="form-control"></button>
                            </div>*@
                        </div>
                    </div>
                    <div class="clearfix mb-md-30 mt-10 mb-10" id="newAddressSet" style="display:none;">
                        <div class="col-md-6"><p></p></div>
                        <div class="col-md-6">
                            <div class="col-md-6">
                                <input type="text" value="" class="form-control form-border-bottom" placeholder="street address" id="addresshome" name="addresshome" />
                            </div>
                            <div class="col-md-6">
                                <input type="text" value="" class="form-control form-border-bottom" id="CoverDetailsSuburb" name="CoverDetailsSuburb" placeholder="Suburb" style="width:100%" />
                            </div>
                        </div>
                    </div>
                    <div class="clearfix mb-md-30 mt-10 mb-10">
                        <p class="col-md-6 col-sm-6 col-xs-12">How much cover do you need to replace your farm contents?</p>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            $@Html.TextBoxFor(p => p.FarmContentsSumInsuredFPObj.SumInsuredFC, new { @style = "width:95%; display:inline-block;", @class = "form-control form-border-bottom vlidationInsure commondatatestComma commondatatestPrice", @data_eiid = @Model.FarmContentsSumInsuredFPObj.EiId, @data_itid = "0", @id = "fcsuminsured" })
                            @Html.HiddenFor(p => p.FarmContentsSumInsuredFPObj.EiId)
                            <label id="FarmContentsSumInsuredFPObj.SumInsuredFC-error" class="error" for="FarmContentsSumInsuredFPObj.SumInsuredFC"></label>
                        </div>
                    </div>
                    <div class="clearfix mb-md-30 mt-10 mb-10">
                        <p class="col-md-6 col-sm-6 col-xs-12">Do you require portable items cover to cover your farm contents Australia wide?</p>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <div class="policy-loc-row">
                                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                                    @Html.RadioButtonFor(m => m.OptPortableItemsFarmContentFPObj.OptPortalableItems, '1', new { @class = "buildingtype commondataradio vlidationInsure", @data_eiid = @Model.OptPortableItemsFarmContentFPObj.EiId, @data_itid = "0", @id = "buildingtype" })
                                    <label for="radio1"><span><span></span></span>Yes</label>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                                    @Html.RadioButtonFor(m => m.OptPortableItemsFarmContentFPObj.OptPortalableItems, '0', new { @class = "buildingtype commondataradio vlidationInsure", @data_eiid = @Model.OptPortableItemsFarmContentFPObj.EiId, @data_itid = "0", @id = "buildingtype1", @checked = "checked" })
                                    <label for="radio1"><span><span></span></span>No</label>
                                </div>
                            </div>
                            @Html.HiddenFor(p => p.OptPortableItemsFarmContentFPObj.EiId)
                            <label id="OptPortableItemsFarmContentFPObj.OptPortalableItems-error" class="error" for="OptPortableItemsFarmContentFPObj.OptPortalableItems"></label>
                        </div>
                    </div>
                    <div class="clearfix mb-md-30 mt-10 mb-10">
                        <p class="col-md-6 col-sm-6 col-xs-12">Please select <a href="#ExcesshomeBuildingModel" role="button" data-target="#ExcesshomeBuildingModel" data-toggle="modal">Excess</a> for farm contents</p>
                        <div class="col-md-3">
                            @Html.DropDownListFor(p => p.ExcessFarmContentFPObj.Excess, listsExcess, new { @class = "form-control form-border-bottom vlidationInsure commondataselection", @data_eiid = @Model.ExcessFarmContentFPObj.EiId, @data_itid = "0", @id = "typeofunits" })
                            @Html.HiddenFor(p => p.ExcessFarmContentFPObj.EiId)
                            <label id="ExcessFarmContentFPObj.Excess-error" class="error" for="ExcessFarmContentFPObj.Excess"></label>
                        </div>
                        @*<div class="col-md-3">
                                <div class="policy-loc-row"><input type="checkbox" name="ImposedFarmcontentsObj.Imposed" id="imposed"><label for="checkbox1"><span><span></span></span>Imposed</label></div>
                            </div>*@
                    </div>
                    <div class="col-md-6 pull-right text-right">
                        <button type="button" class="button-3d nomargin tab-linker quot-next-btn" id="ptabbtn1">Save & Continue</button>
                    </div>
                </div>
                <div id="ptab2" style="display:none;">
                    <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 policy-full-head"><h2>FARM MACHINERY</h2></div>
                    <div class="col-md-12">
                        <h5><a href="#costumModal2" role="button" data-target="#costumModal2" data-toggle="modal"><span class="cover-options">Click here for Farm Machinery definition</span></a></h5>
                    </div>
                    <div id="fmachiModalLabel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="fmachiModalLabel" aria-hidden="false">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h3 class="pull-left">Farm Machinery</h3>
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                        <img src="~/images/close-icon.png" />
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <p>Your Farm Machinery. It is covered against accidental loss or damage that takes place in Australia and is caused by:</p>
                                    <ul style="list-style:none">
                                        <li>(1) Fire;</li>
                                        <li>(2) Earthquake;</li>
                                        <li>(3) Explosion;</li>
                                        <li>(4) Impact by anything that flies or falls from the sky;</li>
                                        <li>(5) Lightning;</li>
                                        <li>(6) Malicious acts;</li>
                                        <li>(7) Storm; or</li>
                                        <li>(8) Volcanoes</li>
                                    </ul>
                                </div>
                                <div class="modal-footer">
                                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <h5><a href="#fmachiModalLabel" role="button" data-target="#fmachiModalLabel" data-toggle="modal"><span class="cover-options">Click here to see the cover available for Farm Machinery</span></a></h5>
                        <div id="costumModal2" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModal2" aria-hidden="false">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h3 class="pull-left">Farm Machinery</h3>
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                            <img src="~/images/close-icon.png" />
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <p>A self-propelled or motor drawn machine or implement designed specifically for agricultural purposes and used primarily for agricultural purposes.</p>
                                        <br />
                                        <p>Farm Machinery does not include any machine or implement that is required to be registered under any motor vehicle registration Act or legislation, whether or not such registration is in place. These can be covered under the motor section. </p>

                                    </div>
                                    <div class="modal-footer">
                                        <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <p class="col-md-12">You can insure unspecified items of Farm Machinery up to $5,000 for any one item</p>
                    <div class="clearfix mb-md-30 mt-10 mb-10">
                        <p class="col-md-6 col-sm-6 col-xs-12">How much cover do you need for unspecified Farm Machinery?</p>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            $@Html.TextBoxFor(p => p.FPUnspecifiedMachineryFMObj.UnspecifiedMachinery, new { @style = "width:95%; display:inline-block;", @class = "form-control form-border-bottom vlidationInsure commondatatestComma commondatatestPrice", @data_eiid = @Model.FPUnspecifiedMachineryFMObj.EiId, @data_itid = "0", @id = "farmstructures", @autocomplete = "off" })
                            @Html.HiddenFor(p => p.FPUnspecifiedMachineryFMObj.EiId)
                            <label id="FPUnspecifiedMachineryFMObj.UnspecifiedMachinery-error" class="error" for="FPUnspecifiedMachineryFMObj.UnspecifiedMachinery"></label>
                        </div>
                    </div>
                    <div class="clearfix mb-md-30 mt-10 mb-10">
                        <p class="col-md-6 col-sm-6 col-xs-12">Please select <a href="#ExcesshomeBuildingModel" role="button" data-target="#ExcesshomeBuildingModel" data-toggle="modal">Excess</a> for unspecified machinery</p>
                        <div class="col-md-3">
                            @Html.DropDownListFor(Model => Model.FPExcessforUMFMObj.Excess, listsExcessFM, new { @class = "form-control form-border-bottom vlidationInsure commondataselection", @data_eiid = @Model.FPExcessforUMFMObj.EiId, @data_itid = "0", @id = "typeofunits" })
                            @Html.HiddenFor(p => p.FPExcessforUMFMObj.EiId)
                            <label id="FPExcessforUMFMObj.Excess-error" class="error" for="FPExcessforUMFMObj.Excess"></label>
                        </div>
                        @*<div class="col-md-3">
                                <div class="policy-loc-row"><input type="checkbox" name="imposed" id="imposed"><label for="checkbox1"><span><span></span></span>Imposed</label></div>
                            </div>*@
                    </div>
                    <div class="clearfix mb-md-30 mt-10 mb-10">
                        <p class="col-md-12">Please list your specified items of machinery (valued over $5,000 per item) below.  </p>
                    </div>
                    <div class="clearfix mb-md-30 mt-10 mb-10">
                        <div class="col-md-12 table-8">
                            <table class="table FPDescriptionsFM" style=" width: 100%; ">
                                <thead>
                                    <tr>
                                        <th class="space_left">Description</th>
                                        <th class="space_left">Year</th>
                                        <th class="space_left">Serial Number</th>
                                        <th class="space_left">Excess</th>
                                        <th class="space_right">Sum Insured</th>
                                        <th class="space_right">&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model.FPDescriptionsFMList != null && Model.FPDescriptionsFMList.Count() > 0)
                                    {
                                        CountTotal = Model.FPDescriptionsFMList.Count();
                                        for (int i = 0; i < Model.FPDescriptionsFMList.Count(); i++)
                                        {
                                            jCount = i + 1;
                                            <tr id="tnameFM-tr-@jCount">
                                                @if (jCount == CountTotal)
                                                {
                                                    <td class="no-border">
                                                        @Html.TextBoxFor(p => p.FPDescriptionsFMObj.DescriptionFM, Model.FPDescriptionsFMList[i].Value, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.FPDescriptionsFMObj.EiId, @data_itid = jCount, @data_regex = "AN", @id = "description", @autocomplete = "off" })
                                                        @Html.HiddenFor(p => p.FPDescriptionsFMObj.EiId)
                                                    </td>
                                                    <td class="no-border">
                                                        @Html.TextBoxFor(p => p.FPYearFMObj.YearFM, Model.FPYearFMList[i].Value, new { @class = "form-control form-border-bottom vlidationInsure commondatatestYearHome", @data_eiid = @Model.FPYearFMObj.EiId, @data_itid = jCount, @data_regex = "N", @id = "year", @autocomplete = "off" })
                                                        @Html.HiddenFor(p => p.FPYearFMObj.EiId)
                                                    </td>
                                                    <td class="no-border">
                                                        @Html.TextBoxFor(p => p.FPSerialNumberFMObj.SerialNumberFM, Model.FPSerialNumberFMList[i].Value, new { @class = "form-control form-border-bottom vlidationInsure commondatatest commondatatestUpperCase", @data_eiid = @Model.FPSerialNumberFMObj.EiId, @data_itid = jCount, @data_regex = "AN", @id = "serialnumber", @autocomplete = "off" })
                                                        @Html.HiddenFor(p => p.FPSerialNumberFMObj.EiId)
                                                    </td>
                                                    <td class="no-border width-100">
                                                        @Html.DropDownListFor(Model => Model.FPExcessFMObj.Excess, @Model.FPExcessFMObj.ExcessList, new { @class = "form-control form-border-bottom vlidationInsure commondataselection", @data_eiid = @Model.FPExcessFMObj.EiId, @data_itid = jCount, @id = "excess", @data_actval = Model.FPExcessFMList[i].Value })
                                                        @Html.HiddenFor(p => p.FPExcessFMObj.EiId)
                                                    </td>
                                                    <td class="no-border">
                                                        $@Html.TextBoxFor(p => p.FPSumOfInsuredFMObj.SuminsuredFM, Model.FPSumOfInsuredFMList[i].Value, new { @style = "display:inline-block;width:90%;", @class = "form-control form-border-bottom vlidationInsure commondatatestComma commondatatestPrice suminsuredCoverFM", @data_eiid = @Model.FPSumOfInsuredFMObj.EiId, @data_itid = jCount, @id = "suminsured", @autocomplete = "off" })

        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Farm Buildings (excluding coolrooms)</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.FarmBuildingExCoolRoomsFPObj.FarmBuildingExcCoolRooms, new { @class = "form-control form-border-bottom commondatatest vlidationInsure", @id = "CoverFB", @data_eiid = @Model.FarmBuildingExCoolRoomsFPObj.EiId, @data_regex = 'N' })
                @Html.HiddenFor(p => p.FarmBuildingExCoolRoomsFPObj.EiId)

                @Html.TextBoxFor(p => p.FarmFencingFPObj.FarmFencing, new { @class = "form-control form-border-bottom commondatatest vlidationInsure", @id = "FPFarmFencing", @data_eiid = @Model.FarmFencingFPObj.EiId, @data_regex = 'N' })
                @Html.HiddenFor(p => p.FarmFencingFPObj.EiId)
                <label id="FarmFencingFPObj.FarmFencing-error" class="error" for="FarmFencingFPObj.FarmFencing"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">

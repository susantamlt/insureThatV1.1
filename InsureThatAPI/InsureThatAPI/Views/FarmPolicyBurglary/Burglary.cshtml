                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptFPPortalableItemsOptObj.PortalbleItemsOpt, '1', new { @class = "buildingtype vlidationInsure commondataradio", @id = "PortalbleItemsOpt1", @data_eiid = @Model.OptFPPortalableItemsOptObj.EiId })<label for="radio1"><span><span></span></span>Yes</label>

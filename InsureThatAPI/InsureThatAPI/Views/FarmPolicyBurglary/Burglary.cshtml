            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Coolrooms</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.CoolRoomsFPObj.CoolRooms, new { @class = "form-control form-border-bottom commondatatest vlidationInsure", @id = "FPCoolRooms", @data_eiid = @Model.CoolRoomsFPObj.EiId, @data_regex = 'N' })
                @Html.HiddenFor(p => p.CoolRoomsFPObj.EiId)
                <label id="CoolRoomsFPObj.CoolRooms-error" class="error" for="CoolRoomsFPObj.CoolRooms"></label>

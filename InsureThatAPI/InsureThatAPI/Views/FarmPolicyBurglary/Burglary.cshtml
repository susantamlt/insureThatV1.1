                <label id="FarmContentsFPObj.FarmContents-error" class="error" for="FarmContentsFPObj.FarmContents"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Hail netting stored in a Farm Structure</p></div>
            <div class="col-md-6 col-sm-6 col-xs-12">
                @Html.TextBoxFor(p => p.HailNettingStoredFPObj.HailNettingStored, new { @class = "form-control form-border-bottom commondatatest vlidationInsure", @id = "FPHailNetting", @data_eiid = @Model.HailNettingStoredFPObj.EiId, @data_regex = 'N' })
                @Html.HiddenFor(p => p.HailNettingStoredFPObj.EiId)
                <label id="HailNettingStoredFPObj.HailNettingStored-error" class="error" for="HailNettingStoredFPObj.HailNettingStored"></label>
                <p class="mini-text">with roof and walls (fire only)</p>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Unspecified machinery	</p></div>
            <div class="col-md-6 col-sm-6 col-xs-12">
                $@Html.TextBoxFor(p => p.UnspecifiedMachineryFPObj.UnspecifiedMachinery, new { @style = "width:95%; display:inline-block;", @class = "form-control form-border-bottom commondatatest vlidationInsure", @id = "FPUnspecMach", @data_eiid = @Model.UnspecifiedMachineryFPObj.EiId, @data_regex = 'N' })
                @Html.HiddenFor(p => p.UnspecifiedMachineryFPObj.EiId)
                <label id="UnspecifiedMachineryFPObj.UnspecifiedMachinery-error" class="error" for="UnspecifiedMachineryFPObj.UnspecifiedMachinery"></label>
                <p class="mini-text">limit $5,000 per item</p>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Specified Items over $5,000 per item</p></div>

                <label id="OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC-error" class="error" for="OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <p class="col-md-6 col-sm-6 col-xs-12">Do you wish to cover theft of your farm machinery?</p>
            <div class="col-md-4 col-sm-4 col-xs-12">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptFPCoverTheftFarmMachineryObj.CoverTheftFarmMachinery, '1', new { @class = "buildingtype vlidationInsure commondataradio", @id = "CoverTheftFarmMachinery1", @data_eiid = @Model.OptFPCoverTheftFarmMachineryObj.EiId })<label for="radio1"><span><span></span></span>Yes</label>

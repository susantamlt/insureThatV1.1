                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC, '1', new { @class = "buildingtype vlidationInsure commondataradio", @id = "CoverTheftFSandFC1", @data_eiid = @Model.OptFPCoverTheftFSAndFCObj.EiId })<label for="radio1"><span><span></span></span>Yes</label>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptFPCoverTheftFSAndFCObj.CoverTheftFSandFC, '0', new { @class = "buildingtype vlidationInsure commondataradio", @id = "CoverTheftFSandFC2", @checked = "checked", @data_eiid = @Model.OptFPCoverTheftFSAndFCObj.EiId })<label for="radio1"><span><span></span></span>No</label>
                    </div>

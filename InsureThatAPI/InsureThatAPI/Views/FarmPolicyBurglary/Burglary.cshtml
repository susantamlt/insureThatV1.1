            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6 col-sm-6 col-xs-12"><p>Farm Contents	</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.FarmContentsFPObj.FarmContents, new { @class = "form-control form-border-bottom commondatatest vlidationInsure", @id = "FPFarmContents", @data_eiid = @Model.FarmContentsFPObj.EiId, @data_regex = 'N' })

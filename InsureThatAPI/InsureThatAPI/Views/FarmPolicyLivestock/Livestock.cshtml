@model InsureThatAPI.Models.FPLivestock
@{
    ViewBag.Title = "Livestock";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<SelectListItem> listsExcess = new List<SelectListItem>();
    listsExcess.Add(new SelectListItem { Value = "200", Text = "$200" });
    listsExcess.Add(new SelectListItem { Value = "500", Text = "$500" });
    listsExcess.Add(new SelectListItem { Value = "1000", Text = "$1,000" });
    listsExcess.Add(new SelectListItem { Value = "2000", Text = "$2,000" });
}
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 policy-indiv-head">
    <h4><span>FARM POLICY</span> / LIVESTOCK</h4>
</div>
<div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 endorsmentview">
    <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 policy-full-head">
        <h2>LIVESTOCK</h2>
    </div>
    <div class="col-md-5 col-sm-5 col-xs-12 pull-right">
        <div class="pull-right">
            <button type="button" href="#savepolicy" role="button" data-target="#savepolicy" data-toggle="modal" class="button-3d nomargin savepolicyP tab-linker" id="savepolicyP" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Save Policy</button>
            <button type="button" href="#referralListModel" role="button" data-target="#referralListModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="referralList" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Referral List</button>
            <button type="button" href="#premiumModel" role="button" data-target="#premiumModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="premium" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Premium</button>
        </div>
    </div>
    <div id="savepolicy" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="savepolicy" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Save Policy</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <p>Do you wish to save the answers you have completed and continue later?</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                    <button class="btn btn-default no-border savepolicyP" data-dismiss="modal" aria-hidden="true"> Save </button>
                </div>
            </div>
        </div>
    </div>
    <div id="referralListModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="referralListModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="pull-left">Referral List</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    @if (Model.Referels != null && Model.Referels.Count() > 0)
                    {
                        for (int j = 0; j < Model.Referels.Count(); j++)
                        {
                            @Model.Referels[j]<br />
                        }
                    }
                    else
                    {
                        <p>No Referral List</p>
                    }
                    <p id="referralList-p"></p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div id="premiumModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="premiumModel" aria-hidden="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Premium Details</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table" style=" width: 100%; ">
                        <thead>
                            <tr>
                                <th class="space_left">Section Name</th>
                                <th class="space_left">Base</th>
                                <th class="space_left">FSL</th>
                                <th class="space_left">GST</th>
                                <th class="space_left">Stamp Duty</th>
                                <th class="space_right">Gross</th>
                            </tr>
                        </thead>
                        <tbody id="premiumModel-p-b">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tbody>
                        <tfoot id="premiumModel-p-f">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 success" id="savepolicyresult"></div>
    @using (Ajax.BeginForm("Livestock", "FarmPolicyLivestock", new { cid = @ViewBag.cid }, new AjaxOptions()
    {
        HttpMethod = "POST"
    }, new { @id = "livestockID", @name = "livestockName" }))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(p => p.CustomerId, new { @id = "ciid" })
        @Html.HiddenFor(p => p.PcId, new { id = "pcidd" })
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <h4>Part A – Death - <a href="#costumModal2" role="button" data-target="#costumModal2" data-toggle="modal">For more information click here</a></h4>
            <div id="costumModal2" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h3 class="pull-left">Death Cover</h3>
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                <img src="~/images/close-icon.png" />
                            </button>
                        </div>
                        <div class="modal-body">
                            <p>Your animal that is shown on the Certificate, is covered against death that arises from an accident or illness that takes place in Australia. This includes humane killing as a result of accident or illness that has been recommended or certified as necessary by a qualified registered practising Veterinary surgeon. Part B Semen and Embryos Your animal semen, embryos and their containers that are shown in the Certificate. They are covered in Australia against accidental loss or damage</p>
                        </div>
                        <div class="modal-footer">
                            <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                        </div>
                    </div>
                </div>
            </div>
            <h5>Please provide the details for each animal to be covered:</h5>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Class of animal</p></div>
            <div class="col-md-6">
                @Html.DropDownListFor(p => p.ClassOfAnimalFPObj.ClassofAnimal, @Model.ClassOfAnimalFPObj.ClassOfAnimalList, "-- Select --", new { @class = "commondataselection vlidationInsure form-control form-border-bottom description", @id = "classofanimal", @data_eiid = @Model.ClassOfAnimalFPObj.EiId })
                @Html.HiddenFor(p => p.ClassOfAnimalFPObj.EiId)
                <label id="ClassOfAnimalFPObj.ClassofAnimal-error" class="error" for="ClassOfAnimalFPObj.ClassofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Type</p></div>
            <div class="col-md-6">
                @Html.DropDownListFor(Model => Model.TypeOfAnimalFPObj.TypeofAnimal, @Model.TypeOfAnimalFPObj.TypeOfAnimalList, "-- Select --", new { @class = "commondataselection vlidationInsure form-control form-border-bottom description", @id = "typeofanimal", @data_eiid = @Model.TypeOfAnimalFPObj.EiId })
                @Html.HiddenFor(p => p.TypeOfAnimalFPObj.EiId)
                <label id="TypeOfAnimalFPObj.TypeofAnimal-error" class="error" for="TypeOfAnimalFPObj.TypeofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>What breed is your animal?</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.BreedOfAnimalFPObj.BreedofAnimal, new { @class = "commondatatest vlidationInsure form-control form-border-bottom", @id = "breedofanimal", @data_eiid = @Model.BreedOfAnimalFPObj.EiId, @data_regex = "AN", @autocomplete = "off" })
                @Html.HiddenFor(p => p.BreedOfAnimalFPObj.EiId)
                <label id="BreedOfAnimalFPObj.BreedofAnimal-error" class="error" for="BreedOfAnimalFPObj.BreedofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>What is the age of the animal?</p></div>
            <div class="col-md-6">
                @Html.DropDownListFor(Model => Model.AgeOfAnimalFPObj.AgeofAnimal, @Model.AgeOfAnimalFPObj.AgeOfAnimalList, "-- Select --", new { @class = "commondataselection vlidationInsure form-control form-border-bottom description", @id = "ageofanimal", @data_eiid = @Model.AgeOfAnimalFPObj.EiId, @autocomplete = "off" })
                @Html.HiddenFor(p => p.AgeOfAnimalFPObj.EiId)
                <label id="AgeOfAnimalFPObj.AgeofAnimal-error" class="error" for="AgeOfAnimalFPObj.AgeofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>What colour are they?</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.ColourOfAnimalFPObj.ColourofAnimal, new { @class = "commondatatest vlidationInsure form-control form-border-bottom", @id = "colourofanimal", @data_eiid = @Model.ColourOfAnimalFPObj.EiId, @data_regex = "AN", @autocomplete = "off" })
                @Html.HiddenFor(p => p.ColourOfAnimalFPObj.EiId)
                <label id="ColourOfAnimalFPObj.ColourofAnimal-error" class="error" for="ColourOfAnimalFPObj.ColourofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10 clssUseofAnimal" @if (Model.ClassOfAnimalFPObj.ClassofAnimal != "4") { <text> style="display:none;" </text>  }>
            <div class="col-md-6"><p>What is your animal used for?</p></div>
            <div class="col-md-6">
                @Html.DropDownListFor(Model => Model.UseOfAnimalFPObj.UseofAnimal, @Model.UseOfAnimalFPObj.UseOfAnimalList, "-- Select --", new { @class = "commondataselection vlidationInsure form-control form-border-bottom description", @id = "useofanimal", @data_eiid = @Model.UseOfAnimalFPObj.EiId })
                @Html.HiddenFor(p => p.UseOfAnimalFPObj.EiId)
                <label id="UseOfAnimalFPObj.UseofAnimal-error" class="error" for="UseOfAnimalFPObj.UseofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Describe their brand</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.DescBrandOfAnimalFPObj.BrandofAnimal, new { @class = "commondatatest form-control form-border-bottom vlidationInsure", @id = "brandofanimaldes", @data_eiid = @Model.DescBrandOfAnimalFPObj.EiId, @data_regex = "AN", @autocomplete = "off" })
                @Html.HiddenFor(p => p.DescBrandOfAnimalFPObj.EiId)
                <label id="DescBrandOfAnimalFPObj.BrandofAnimal-error" class="error" for="DescBrandOfAnimalFPObj.BrandofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Describe their markings</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.DescMarksOfAnimalFPObj.MarkingsofAnimal, new { @class = "commondatatest form-control form-border-bottom vlidationInsure", @id = "markingsofanimal", @data_eiid = @Model.DescMarksOfAnimalFPObj.EiId, @data_regex = "AN", @autocomplete = "off" })
                @Html.HiddenFor(p => p.DescMarksOfAnimalFPObj.EiId)
                <label id="DescMarksOfAnimalFPObj.MarkingsofAnimal-error" class="error" for="DescMarksOfAnimalFPObj.MarkingsofAnimal"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Is the animal in sound health?</p></div>
            <div class="col-md-6">
                <div class="policy-loc-row">
                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptSoundHealthofAnimalFPObj.HealthofAnimal, '1', new { @class = "commondataradio vlidationInsure healthofAnimal", @id = "buildingtype", @checked = "checked", @data_eiid = @Model.OptSoundHealthofAnimalFPObj.EiId })
                        <label for="radio1"><span><span></span></span>Yes</label>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptSoundHealthofAnimalFPObj.HealthofAnimal, '0', new { @class = "commondataradio vlidationInsure healthofAnimal", @id = "buildingtype1", @data_eiid = @Model.OptSoundHealthofAnimalFPObj.EiId })
                        <label for="radio1"><span><span></span></span>No</label>
                    </div>
                </div>
                <label id="OptSoundHealthofAnimalFPObj.HealthofAnimal-error" class="error" for="OptSoundHealthofAnimalFPObj.HealthofAnimal"></label>
                @Html.HiddenFor(p => p.OptSoundHealthofAnimalFPObj.EiId)
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10 hltDescription" @if (Model.OptSoundHealthofAnimalFPObj.HealthofAnimal != "0") { <text> style="display:none;" </text> }>
            <div class="col-md-6"><p>If no, please provide details</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.DescSoundHealthofAnimalFPObj.Description, new { @class = "commondatatest vlidationInsure form-control form-border-bottom", @id = "helthofdescription", @data_eiid = @Model.DescSoundHealthofAnimalFPObj.EiId, @data_regex = "AN", @autocomplete = "off" })
                @Html.HiddenFor(p => p.DescSoundHealthofAnimalFPObj.EiId)
                <label id="DescSoundHealthofAnimalFPObj.Description-error" class="error" for="DescSoundHealthofAnimalFPObj.Description"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Are there any contagious or infectious diseases in any of your stock?</p></div>
            <div class="col-md-6">
                <div class="policy-loc-row">
                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptDiseaseOfAnimalFPObj.DiseaseofAnimal, '1', new { @class = "commondataradio vlidationInsure diseaseofAnimal", @id = "buildingtype", @data_eiid = @Model.OptDiseaseOfAnimalFPObj.EiId })
                        <label for="radio1"><span><span></span></span>Yes</label>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptDiseaseOfAnimalFPObj.DiseaseofAnimal, '0', new { @class = "commondataradio vlidationInsure diseaseofAnimal", @id = "buildingtype1", @checked = "checked", @data_eiid = @Model.OptDiseaseOfAnimalFPObj.EiId })
                        <label for="radio1"><span><span></span></span>No</label>
                    </div>
                </div>
                <label id="OptDiseaseOfAnimalFPObj.DiseaseofAnimal-error" class="error" for="OptDiseaseOfAnimalFPObj.DiseaseofAnimal"></label>
                @Html.HiddenFor(p => p.OptDiseaseOfAnimalFPObj.EiId)
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10 hltDiseaseofAnimal" @if (Model.OptDiseaseOfAnimalFPObj.DiseaseofAnimal != "1") { <text> style="display:none;" </text> }>
            <div class="col-md-6"><p>If yes, please provide details</p></div>
            <div class="col-md-6">
                @Html.TextBoxFor(p => p.DescDiseaseOfAnimalFPObj.Description, new { @class = "commondatatest vlidationInsure form-control form-border-bottom", @id = "diseasesofanimal", @data_eiid = @Model.DescDiseaseOfAnimalFPObj.EiId, @data_regex = "AN", @autocomplete = "off" })
                @Html.HiddenFor(p => p.DescDiseaseOfAnimalFPObj.EiId)
                <label id="DescDiseaseOfAnimalFPObj.Description-error" class="error" for="DescDiseaseOfAnimalFPObj.Description"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>Is the animal syndicated?</p></div>
            <div class="col-md-6">
                <div class="policy-loc-row">
                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptAnimalSyndicatedFPObj.AnimalSyndicated, '1', new { @class = "commondataradio vlidationInsure buildingtype", @id = "OptAnimalSyndicatedFP1", @data_eiid = @Model.OptAnimalSyndicatedFPObj.EiId })
                        <label for="radio1"><span><span></span></span>Yes</label>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                        @Html.RadioButtonFor(m => m.OptAnimalSyndicatedFPObj.AnimalSyndicated, '0', new { @class = "commondataradio vlidationInsure buildingtype", @id = "OptAnimalSyndicatedFP2", @checked = "checked", @data_eiid = @Model.OptAnimalSyndicatedFPObj.EiId })
                        <label for="radio1"><span><span></span></span>No</label>
                    </div>
                </div>
                <label id="OptAnimalSyndicatedFPObj.AnimalSyndicated-error" class="error" for="OptAnimalSyndicatedFPObj.AnimalSyndicated"></label>
                @Html.HiddenFor(p => p.OptAnimalSyndicatedFPObj.EiId)
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10">
            <div class="col-md-6"><p>For how much do you wish to insure your animal?</p></div>
            <div class="col-md-6">
                $@Html.TextBoxFor(p => p.SumInsuredLivestockFPObj.SumInsured, new { @style = "width:95%; display:inline-block;", @class = "vlidationInsure commondatatestComma commondatatestPrice form-control form-border-bottom", @id = "suminsuredlivestock", @data_eiid = @Model.SumInsuredLivestockFPObj.EiId, @data_regex = 'N', @autocomplete = "off" })
                @Html.HiddenFor(p => p.SumInsuredLivestockFPObj.EiId)
                <label id="SumInsuredLivestockFPObj.SumInsured-error" class="error" for="SumInsuredLivestockFPObj.SumInsured"></label>
            </div>
        </div>
        <div class="clearfix mb-md-30 mt-10 mb-10 ">
            <div class="col-md-6"><p>You can choose the following optional extras.  Click on each option to obtain further details</p></div>
            <div class="col-md-6" id="obtainFurtherDetailsCheckbox">
                <div class="policy-loc-row">
                    <div class="policy-loc-row col-md-6 col-sm-6 col-xs-6">@Html.CheckBox("obtainFurtherDetails", new { @class = "vlidationInsure commondatacheckbox buildingtype", @id = "infertility", @data_eiid = "63359" })<label for="checkbox1"><span><span></span></span><a href="#costumModal3" role="button" data-target="#costumModal3" data-toggle="modal">Infertility</a></label></div>
                    <div class="policy-loc-row col-md-6 col-sm-6 col-xs-6">@Html.CheckBox("obtainFurtherDetails", new { @class = "vlidationInsure commondatacheckbox buildingtype", @id = "LossofUse", @data_eiid = "63361" })<label for="checkbox2"><span><span></span></span><a href="#costumModal4" role="button" data-target="#costumModal4" data-toggle="modal">Loss of Use</a></label></div>
                    <div class="policy-loc-row col-md-6 col-sm-6 col-xs-6">@Html.CheckBox("obtainFurtherDetails", new { @class = "vlidationInsure commondatacheckbox buildingtype", @id = "theft", @data_eiid = "63363" })<label for="checkbox3"><span><span></span></span><a href="#costumModal5" role="button" data-target="#costumModal5" data-toggle="modal">Theft</a></label></div>
                    <div class="policy-loc-row col-md-6 col-sm-6 col-xs-6">@Html.CheckBox("obtainFurtherDetails", new { @class = "vlidationInsure commondatacheckbox buildingtype", @id = "UnbornFoal", @data_eiid = "63365" })<label for="checkbox4"><span><span></span></span><a href="#costumModal6" role="button" data-target="#costumModal6" data-toggle="modal">Unborn Foal</a></label></div>
                </div>
                <label id="obtainFurtherDetails-error" class="error" for="obtainFurtherDetails"></label>
            </div>
            @*<script type="text/javascript">
                    $(document).ready(function () {
                        $("#obtainFurtherDetailsCheckbox input[type='hidden'][name='obtainFurtherDetails']").remove();
                    });
                </script>*@
            <div id="costumModal3" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h3 class="pull-left">Infertility</h3>
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                <img src="~/images/close-icon.png" />
                            </button>
                        </div>
                        <div class="modal-body">
                            <p>If Your animal becomes infertile during the period of cover We will pay You the market value of Your animal immediately before the infertility took place or the Sum Insured whichever is the lesser, from which We will deduct the sum received for the sale of the animal. A registered veterinary surgeon must certify that:</p>
                            <ul style="list-style:none">
                                <li>[a] The infertility is permanent; and</li>
                                <li>[b] The infertility was caused by an accident or illness; and</li>
                                <li>[c] Your animal has not been able to produce offspring for six months.</li>
                            </ul>
                        </div>
                        <div class="modal-footer">
                            <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                        </div>
                    </div>
                </div>
            </div>
            <div id="costumModal4" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h3 class="pull-left">Loss of Use</h3>
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                <img src="~/images/close-icon.png" />
                            </button>
                        </div>
                        <div class="modal-body">
                            <p>We will extend Part A Death of this section to cover Your animal against a disability that stops it being able to fulfil the purpose for which it was kept. The disability may be physical or cosmetic. This cover does not include loss of use arising from infertility. A registered veterinary surgeon must certify that:</p>
                            <ul style="list-style:none">
                                <li>[a] The disability is permanent; and</li>
                                <li>[b] The disability was caused by an accident or illness.</li>
                            </ul>
                            <p>We will pay You the market value of Your animal immediately before the loss of use took place or the Sum Insured whichever is the lesser, from which We will deduct the sum received for the sale of the animal.</p>
                        </div>
                        <div class="modal-footer">
                            <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                        </div>
                    </div>
                </div>
            </div>
            <div id="costumModal5" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="false">
                <div class="modal-dialog">
                    <div class="modal-content">

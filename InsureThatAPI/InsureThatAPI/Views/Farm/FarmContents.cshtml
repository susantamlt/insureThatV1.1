@model InsureThatAPI.Models.FarmContents
@{
    ViewBag.Title = "Farm Contents / Farm Property Cover";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<SelectListItem> listsExcessFC = new List<SelectListItem>();
    listsExcessFC.Add(new SelectListItem { Value = "250", Text = "$250" });
    listsExcessFC.Add(new SelectListItem { Value = "500", Text = "$500" });
    listsExcessFC.Add(new SelectListItem { Value = "1000", Text = "$1,000" });
    listsExcessFC.Add(new SelectListItem { Value = "2000", Text = "$2,000" });
    listsExcessFC.Add(new SelectListItem { Value = "5000", Text = "$5,000" });
    listsExcessFC.Add(new SelectListItem { Value = "10000", Text = "$10,000" });
    List<SelectListItem> listsExcessFM1 = new List<SelectListItem>();
    listsExcessFM1.Add(new SelectListItem { Value = "250", Text = "$250" });
    listsExcessFM1.Add(new SelectListItem { Value = "750", Text = "$750" });
    listsExcessFM1.Add(new SelectListItem { Value = "1250", Text = "$1,250" });
    listsExcessFM1.Add(new SelectListItem { Value = "2500", Text = "$2,250" });
    List<SelectListItem> listsExcessFM2 = new List<SelectListItem>();
    listsExcessFM2.Add(new SelectListItem { Value = "250", Text = "$250" });
    listsExcessFM2.Add(new SelectListItem { Value = "500", Text = "$500" });
    listsExcessFM2.Add(new SelectListItem { Value = "1000", Text = "$1,000" });
    listsExcessFM2.Add(new SelectListItem { Value = "2000", Text = "$2,000" });
}
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 policy-indiv-head">
    <h4><span>RURAL LIFESTYLE POLICY</span> / FARM PROPERTY</h4>
</div>
<div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 endorsmentview">
    <div class="col-md-5 col-sm-5 col-xs-12 pull-right">
        <div class="pull-right">
            <button type="button" href="#savepolicy" role="button" data-target="#savepolicy" data-toggle="modal" class="button-3d nomargin tab-linker" id="savepolicyP" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Save Policy</button>
            <button type="button" href="#referralListModel" role="button" data-target="#referralListModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="referralList" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Referral List</button>
            <button type="button" href="#premiumModel" role="button" data-target="#premiumModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="premium" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Premium</button>
        </div>
    </div>
    <div id="savepolicy" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="savepolicy" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Save Policy</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <p>Do you wish to save and exit.</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                    <button class="btn btn-default no-border savepolicyP" data-dismiss="modal" aria-hidden="true"> Save </button>
                </div>
            </div>
        </div>
    </div>
    <div id="referralListModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="referralListModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="pull-left">Referral List</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    @if (Model.Referels != null && Model.Referels.Count() > 0)
                    {
                        for (int j = 0; j < Model.Referels.Count(); j++)
                        {
                            @Model.Referels[j]<br />
                        }
                    }
                    else
                    {
                        <p>No Referral List</p>
                    }
                    <p id="referralList-p"></p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div id="premiumModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="premiumModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Premium Details</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table" style=" width: 100%; ">
                        <thead>
                            <tr>
                                <th class="space_left">Section Name</th>
                                <th class="space_left">Base</th>
                                <th class="space_left">FSL</th>
                                <th class="space_left">GST</th>
                                <th class="space_left">Stamp Duty</th>
                                <th class="space_right">Gross</th>
                            </tr>
                        </thead>
                        <tbody id="premiumModel-p-b">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tbody>
                        <tfoot id="premiumModel-p-f">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div class="mt-40 mb-40 row"></div>
    <div class="clearfix"></div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 success" id="savepolicyresult"></div>
    <div id="processTabs">
        <ul class="process-steps clearfix col-sm-12">
            <li>
                <a href="javascript:void(0)" id="ptb1">Farm Structures and Farm Contents</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb2">Farm Machinery</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb3">Livestock</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb4">Harvested Crops and Beehives</a>
            </li>
        </ul>
        @using (Ajax.BeginForm("FarmContents", "Farm", new { cid = @ViewBag.cid }, new AjaxOptions()
        {
            HttpMethod = "POST"

        }, new { @id = "farmContents", @name = "farmContents" }))
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(p => p.CustomerId, new { @id = "ciid" })
            @Html.HiddenFor(p => p.PcId, new { id = "pcidd" })
            <div id="ptab1">
                <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 policy-full-head"><h2 style="font-size: 21px;margin-bottom: 0;">FARM STRUCTURES AND FARM CONTENTS</h2></div>
                <div class="mt-10 mb-10 col-md-12">
                    <p><a href="#FarmContentsModal3" role="button" data-target="#FarmContentsModal3" data-toggle="modal">Click here to see the information about Farm Contents?</a></p>
                    <p><a href="#FarmStructuresModal3" role="button" data-target="#FarmStructuresModal3" data-toggle="modal">Click here to see the information about Farm Structures</a></p>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <h5>Farm Building Details</h5>
                    <p>What type of Farm Building do you need to cover?</p>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-12 table-8">
                        <table class="table" style=" width: 100%; ">
                            <thead>
                                <tr>
                                    <th class="space_left">Description</th>
                                    <th class="space_left">Year</th>
                                    <th class="space_left">Construction</th>
                                    <th class="space_left">Coolroom</th>
                                    <th class="space_left">Sum Insured</th>
                                    <th class="space_right">&nbsp;</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr id="tnameFC-tr-1">
                                    <td class="no-border">
                                        @*@Html.DropDownListFor(Model => Model.DescriptionFCObj.Description, @Model.DescriptionFCObj.DescriptionList, "-- Select --", new { @class = "form-control form-border-bottom description vlidationInsure commondataselection", @data_eiid = @Model.DescriptionFCObj.EiId, @data_itid = "0", @id = "description1" })*@
                                        @Html.TextBoxFor(p => p.DescriptionFCObj.Description, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @data_eiid = @Model.DescriptionFCObj.EiId, @data_itid = "0", @id = "description1", @data_regex = "AN" })
                                        @Html.HiddenFor(p => p.DescriptionFCObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        @Html.TextBoxFor(p => p.YearObj.Year, new { @class = "form-control form-border-bottom vlidationInsure commondatatestYearHome", @data_eiid = @Model.YearObj.EiId, @data_itid = "0", @id = "year1", @data_regex = "N" })
                                        @Html.HiddenFor(p => p.YearObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        @Html.DropDownListFor(Model => Model.MaterialsObj.Materials, @Model.MaterialsObj.MaterialsList, "-- Select --", new { @class = "form-control form-border-bottom vlidationInsure commondataselection", @data_eiid = @Model.MaterialsObj.EiId, @data_itid = "0", @id = "materials1" })
                                        @Html.HiddenFor(p => p.MaterialsObj.EiId)
                                    </td>
                                    <td class="no-border">
                                        <div class="policy-loc-row">
                                            <input type="checkbox" name="CoolroomFcObj.Coolroom" id="coolroom1" class="vlidationInsure"><label for="checkbox1"><span><span></span></span></label>
                                        </div>
                                    </td>
                                    <td class="no-border">
                                        $@Html.TextBoxFor(p => p.SuminsuredObj.Suminsured, new { @style = "display:inline-block;width:90%;", @class = "form-control form-border-bottom  vlidationInsure sumoftotalcover commondatatestComma commondatatestPrice", @data_eiid = @Model.SuminsuredObj.EiId, @data_itid = "0", @id = "suminsured1", @data_regex = "N" })
                                        @Html.HiddenFor(p => p.SuminsuredObj.EiId)
                                    </td>
                                    <td class="no-border" id="tnameFC-1"><a href="javascript:void(0)" onclick="tnameAddFC(1)" class="button-3d">+</a></td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td class="no-border"><label id="DescriptionFCObj.Description-error" class="error" for="DescriptionFCObj.Description"></label></td>
                                    <td class="no-border"><label id="YearObj.Year-error" class="error" for="YearObj.Year"></label></td>
                                    <td class="no-border"><label id="MaterialsObj.Materials-error" class="error" for="MaterialsObj.Materials"></label></td>
                                    <td class="no-border"><label id="CoolroomFcObj.Coolroom-error" class="error" for="CoolroomFcObj.Coolroom"></label></td>
                                    <td class="no-border"><label id="SuminsuredObj.Suminsured-error" class="error" for="SuminsuredObj.Suminsured"></label></td>
                                    <td class="no-border">&nbsp;</td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <p class="col-md-8 col-sm-8 col-xs-12">Do you confirm that all Farm Structures to be insured are structurally sound, well maintained and have no unrepaired damage?</p>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="policy-loc-row">
                            @if (@Model.confirmfsObj.Confirm == "1")
                            {
                                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                                    @Html.RadioButtonFor(m => m.confirmfsObj.Confirm, '1', new { @class = "buildingtype vlidationInsure commondataradio", @data_eiid = @Model.confirmfsObj.EiId, @id = "buildingtype", @checked = "checked" })
                                    <label for="radio1"><span><span></span></span>Yes</label>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                                    @Html.RadioButtonFor(m => m.confirmfsObj.Confirm, '0', new { @class = "buildingtype vlidationInsure commondataradio", @data_eiid = @Model.confirmfsObj.EiId, @id = "buildingtype1" })
                                    <label for="radio1"><span><span></span></span>No</label>
                                </div>
                            }
                            else if (@Model.confirmfsObj.Confirm == "0")
                            {
                                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                                    @Html.RadioButtonFor(m => m.confirmfsObj.Confirm, '1', new { @class = "buildingtype vlidationInsure commondataradio", @data_eiid = @Model.confirmfsObj.EiId, @id = "buildingtype" })
                                    <label for="radio1"><span><span></span></span>Yes</label>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                                    @Html.RadioButtonFor(m => m.confirmfsObj.Confirm, '0', new { @class = "buildingtype vlidationInsure commondataradio", @data_eiid = @Model.confirmfsObj.EiId, @id = "buildingtype1", @checked = "checked" })
                                    <label for="radio1"><span><span></span></span>No</label>
                                </div>
                            }
                            else
                            {
                                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                                    @Html.RadioButtonFor(m => m.confirmfsObj.Confirm, '1', new { @class = "buildingtype vlidationInsure commondataradio", @data_eiid = @Model.confirmfsObj.EiId, @id = "buildingtype" })
                                    <label for="radio1"><span><span></span></span>Yes</label>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">
                                    @Html.RadioButtonFor(m => m.confirmfsObj.Confirm, '0', new { @class = "buildingtype vlidationInsure commondataradio", @data_eiid = @Model.confirmfsObj.EiId, @id = "buildingtype1" })
                                    <label for="radio1"><span><span></span></span>No</label>
                                </div>
                            }
                            @Html.HiddenFor(p => p.confirmfsObj.EiId)
                        </div>
                        <label id="confirmfsObj.Confirm-error" class="error" for="confirmfsObj.Confirm"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-6"><p>Please advise the cost per kilometre to replace your fencing</p></div>
                    <div class="col-md-6">
                        $@Html.TextBoxFor(p => p.FarmfencingObj.Farmfencing, new { @style = "display:inline-block;width:95%;", @class = "form-control form-border-bottom vlidationInsure commondatatestComma commondatatestPrice", @data_eiid = @Model.FarmfencingObj.EiId, @id = "farmfencing", @data_regex = "N" })
                        @Html.HiddenFor(p => p.FarmfencingObj.EiId)
                        <label id="FarmfencingObj.Farmfencing-error" class="error" for="FarmfencingObj.Farmfencing"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-6"><p>What is the total replacement cost of fencing cover you would like?</p></div>
                    <div class="col-md-6">
                        $@Html.TextBoxFor(p => p.FarmcencingTcObj.Totalcover, new { @style = "display:inline-block;width:95%;", @class = "form-control form-border-bottom vlidationInsure commondatatestComma commondatatestPrice", @data_eiid = @Model.FarmcencingTcObj.EiId, @id = "totalcover", @data_regex = "N" })
                        @Html.HiddenFor(p => p.FarmcencingTcObj.EiId)
                        <label id="FarmcencingTcObj.Totalcover-error" class="error" for="FarmcencingTcObj.Totalcover"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-6">
                        <p>
                            Please advise how much you would like the rest of your <a href="#FarmStructuresModal3" role="button" data-target="#FarmStructuresModal3" data-toggle="modal">Farm Structures</a> to be insured<br />
                            <span class="mini-text">(includes covered ways, exterior light fittings, irrigation systems, pens, sheep dips, stockyards, pipes, cables, power and telephone poles, silos, solar power units, tanks and windmills)</span>
                        </p>
                    </div>
                    <div id="FarmStructuresModal3" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="FarmStructuresModal3" aria-hidden="false">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h3 class="pull-left">Farm Structures</h3>
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                        <img src="~/images/close-icon.png" />
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <p>Farm Structures means:</p>
                                    <p>All farm buildings or permanent structures including:</p>
                                    <ul style="list-style:none">
                                        <li>(1) Covered ways.</li>
                                        <li>(2) Exterior light fittings and their controllers.</li>
                                        <li>(3) Irrigation systems and their controllers.</li>
                                        <li>(4) Paved or concrete (but not gravelled) driveways, paths terraces and courts.</li>
                                        <li>(5) Fixed pens, sheep dips and stockyards.
                                        <li>(6) Pipes and cables.</li>
                                        <li>(7) Power and telephone poles.</li>
                                        <li>(8) Fixed silos.</li>
                                        <li>(9) Fixed solar power units and their controllers and inverters.</li>
                                        <li>(10) Tanks.</li>
                                        <li>(11) Fixed Farm Fencing but not hedges.</li>
                                        <li>(12) Windmills but not wind turbines.</li>
                                    </ul>
                                    <p>It does not mean any:</p>
                                    <ul style="list-style:none">
                                        <li>(1) Home Buildings.</li>
                                        <li>(2) Glasshouses, shade houses and structures designed to house growing plants.</li>
                                    </ul>

                                </div>
                                <div class="modal-footer">
                                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        $@Html.TextBoxFor(p => p.FarmstructuresObj.Farmstructures, new { @style = "display:inline-block;width:95%;", @class = "form-control form-border-bottom vlidationInsure commondatatestComma commondatatestPrice", @data_eiid = @Model.FarmstructuresObj.EiId, @id = "farmstructures", @data_regex = "N" })
                        @Html.HiddenFor(p => p.FarmstructuresObj.EiId)
                        <label id="FarmstructuresObj.Farmstructures-error" class="error" for="FarmstructuresObj.Farmstructures"></label>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12">
                    <div class="col-md-6"><p>How much cover do you need to replace your <a href="#FarmContentsModal3" role="button" data-target="#FarmContentsModal3" data-toggle="modal">Farm Contents?</a></p></div>
                    <div class="col-md-6">
                        $@Html.TextBoxFor(p => p.FarmContentFcObj.Farmcontents, new { @style = "display:inline-block;width:95%;", @class = "form-control form-border-bottom vlidationInsure commondatatestComma commondatatestPrice", @data_eiid = @Model.FarmContentFcObj.EiId, @id = "farmstructuresFc", @data_regex = "N" })
                        @Html.HiddenFor(p => p.FarmContentFcObj.EiId)
                        <label id="FarmContentFcObj.Farmcontents-error" class="error" for="FarmContentFcObj.Farmcontents"></label>
                    </div>
                </div>
                <div id="FarmContentsModal3" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="FarmContentsModal3" aria-hidden="false">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h3 class="pull-left">Farm Contents</h3>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                    <img src="~/images/close-icon.png" />
                                </button>
                            </div>
                            <div class="modal-body">
                                <ul style="list-style:none">
                                    <li>(1) All farm contents on Your farm; and</li>
                                    <li>(2) Any office equipment in Your Home Buildings.</li>
                                    <li>(3) Portable pens, sheep dips and stockyards;</li>
                                    <li>(4) Portable silos;</li>
                                    <li>(5) Portable solar power units</li>
                                    <li>(6) Portable farm fencing.</li>
                                </ul>
                                <p>It does not mean any:</p>
                                <ul style="list-style:none">
                                    <li>(1) Item that can be more specifically insured under another section of this policy;</li>
                                    <li>(2) Home Contents;</li>
                                    <li>(3) Valuables;</li>
                                    <li>(4) Mobile phone, laptop computer, hand held computer or global positioning system unless they are used for or connected to Your business as specified in the Certificate;</li>
                                    <li>(5) Aircraft; balloon; drone; craft that travels on a cushion of air; rocket or an accessory to such items;</li>
                                    <li>(6) Business book, document, record or plan except for the material on which they are written or stored. We do cover blank stationery.</li>
                                    <li>(7) Cash, cheque, postal note, money order, stamp, credit card voucher or negotiable instrument;</li>
                                    <li>(8) Jewellery, fur, precious metal or stone;</li>
                                    <li>(9) Living creature;</li>
                                    <li>(10) Farm Machinery nor self-propelled or motor drawn machine or implement not designed specifically for agricultural purposes or used primarily for agricultural purposes;</li>
                                    <li>(11) Harvested crops;</li>
                                    <li>(12) Animal semen or embryos, and their containers (cover available under Livestock in the Farm Property section);</li>
                                    <li>(13) Beehives (cover available under Beehives in the Farm Property section);</li>
                                    <li>(14) Motor vehicle, motorcycle, motor scooter or an accessory to such items;</li>
                                    <li>(15) Property that is described under Farm Structures;</li>
                                    <li>(16) Vehicle that runs on rails or an accessory to such a vehicle;</li>
                                    <li>(17) Watercraft or an accessory to a watercraft</li>
                                </ul>
                            </div>
                            <div class="modal-footer">
                                <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="mt-10 mb-10 col-md-12 ">
                    <div class="col-md-6"><p>Please select <a href="#ExcesshomeBuildingModel" role="button" data-target="#ExcesshomeBuildingModel" data-toggle="modal">Excess</a></p></div>
                    <div class="col-md-3">
                        @Html.DropDownListFor(Model => Model.ExcessesFpcObj.Excess, listsExcessFC, new { @class = "gray-bg-underline vlidationInsure form-control form-border-bottom field-length-sm commondataselection", @data_eiid = @Model.ExcessesFpcObj.EiId, @id = "excessFC" })
                        @Html.HiddenFor(p => p.ExcessesFpcObj.EiId)
                        <label id="ExcessesFpcObj.Excess-error" class="error" for="ExcessesFpcObj.Excess"></label>
                    </div>
                </div>
                <div class="col-md-6 pull-right text-right">
                    <button type="button" class="button-3d nomargin tab-linker quot-next-btn homedescrtn" id="ptabbtn1">Save & Continue</button>
                </div>
            </div>
            <div id="ptab2">
                <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 policy-full-head"><h2>FARM MACHINERY</h2></div>
                <div class="mt-10 mb-10 col-md-12">

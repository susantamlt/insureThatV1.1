@model InsureThatAPI.Models.MCVehicleDescription
@{
    ViewBag.Title = "Vehicle Description";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 policy-indiv-head">
    <h4><span>FARM PROTECTION POLICY</span> / MOTOR</h4>
</div><div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 endorsmentview">
    <div class="col-md-5 col-sm-5 col-xs-12 pull-right">
        <div class="pull-right">
            <button type="button" href="#savepolicy" role="button" data-target="#savepolicy" data-toggle="modal" class="button-3d nomargin tab-linker" id="savepolicyP" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Save Policy</button>
            <button type="button" href="#referralListModel" role="button" data-target="#referralListModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="referralList" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Referral List</button>
            <button type="button" href="#premiumModel" role="button" data-target="#premiumModel" data-toggle="modal" class="button-3d nomargin tab-linker" id="premium" style="width:auto;border:none;background-color:#000000;color:#FFFFFF;margin-top:10px;line-height:22px;">Premium</button>
        </div>
    </div>
    <div id="savepolicy" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="savepolicy" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Save Policy</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <p>Do you wish to save your policy till now and continue later.</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                    <button class="btn btn-default no-border savepolicyP" data-dismiss="modal" aria-hidden="true"> Save </button>
                </div>
            </div>
        </div>
    </div>
    <div id="referralListModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="referralListModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="pull-left">Referral List</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    @if (Model.Referels != null && Model.Referels.Count() > 0)
                    {
                        for (int j = 0; j < Model.Referels.Count(); j++)
                        {
                            @Model.Referels[j]<br />
                        }
                    }
                    else
                    {
                        <p>No Referral List</p>
                    }
                    <p id="referralList-p"></p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div id="premiumModel" class="modal" data-easein="fadeIn" tabindex="-1" role="dialog" aria-labelledby="premiumModel" aria-hidden="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="padding: 15px 26px 0px;">
                    <h3 class="pull-left">Premium Details</h3>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        <img src="~/images/close-icon.png" />
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table" style=" width: 100%; ">
                        <thead>
                            <tr>
                                <th class="space_left">Section Name</th>
                                <th class="space_left">Base</th>
                                <th class="space_left">Fsl</th>
                                <th class="space_left">Gst</th>
                                <th class="space_left">Stamp Duty</th>
                                <th class="space_right">Gross</th>
                            </tr>
                        </thead>
                        <tbody id="premiumModel-p-b">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tbody>
                        <tfoot id="premiumModel-p-f">
                            <tr>
                                <td colspan="6">No Premium Details</td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default no-border" data-dismiss="modal" aria-hidden="true"> Back </button>
                </div>
            </div>
        </div>
    </div>
    <div class="mt-40 mb-40 row"></div>
    <div class="clearfix"></div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 success" id="savepolicyresult"></div>
    <div id="processTabs">
        <ul class="process-steps clearfix col-sm-12" style="padding:0px 0px 0px 10px !important;">
            <li>
                <a href="javascript:void(0)" id="ptb1">Vehicle Description</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb2">Additional Details</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb3">Drivers</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb4">Cover Details</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb5">Optional Extras and Excesses</a>
            </li>
            <li>
                <a href="javascript:void(0)" id="ptb6">Interested Parties</a>
            </li>
        </ul>
        @using (Ajax.BeginForm("VehicleDescription", "FarmMotors", new { cid = @ViewBag.CustomerId }, new AjaxOptions()
        {
            HttpMethod = "POST"
        }, new { @id = "vehicleDescription", @name = "vehicleDescription" }))
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(p => p.CustomerId, new { @id = "ciid" })
            @Html.HiddenFor(p => p.PcId, new { id = "pcidd" })
            <div id="homeBuildContain">
                <div id="ptab1">
                    <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 policy-full-head"><h2>VEHICLE DESCRIPTION</h2></div>
                    <div class="mt-10 mb-10 col-md-12">
                        <h5>Glass's Listed Vehicles</h5>
                    </div>
                    <div class="mt-10 mb-10 col-md-12 ">
                        <p class="col-md-3 col-sm-12 col-xs-12 policy-loc-row">Category</p>
                        <div class="col-md-9 col-sm-12 col-xs-12">
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.LscategoryObj.Category, '0', new { @class = "commondataradio vlidationInsure", @data_eiid = @Model.LscategoryObj.EiId, @id = "buildingtype" })<label for="radio1"><span><span></span></span>Passenger</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.LscategoryObj.Category, '1', new { @class = "commondataradio vlidationInsure", @data_eiid = @Model.LscategoryObj.EiId, @id = "holidayhome" })<label for="radio1"><span><span></span></span>Light Commercial</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.LscategoryObj.Category, '2', new { @class = "commondataradio vlidationInsure", @data_eiid = @Model.LscategoryObj.EiId, @id = "other" })<label for="radio1"><span><span></span></span>Heavy</label></div>
                            @Html.HiddenFor(p => p.LscategoryObj.EiId)
                            <label id="LscategoryObj.Category-error" class="error" for="LscategoryObj.Category"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What is the make of your vehicle?</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.McmakeObj.Make, @Model.McmakeObj.MakeList, new { @class = "form-control form-border-bottom vehicle vlidationInsure commondataselection", @data_eiid = @Model.McmakeObj.EiId, @id = "vehiclemake" })
                            @Html.HiddenFor(p => p.McmakeObj.EiId)
                            <label id="McmakeObj.Make-error" class="error" for="McmakeObj.Make"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What year was it manufactured?</p></div>
                        <div class="col-md-6">
                            @Html.TextBoxFor(p => p.McyearObj.Year, new { @class = "form-control form-border-bottom year vehicle vlidationInsure commondatatest", @id = "year1", @data_eiid = @Model.McyearObj.EiId, @readonly = "readonly", @data_regex = "N" })
                            @Html.HiddenFor(p => p.McyearObj.EiId)
                            <label id="McyearObj.Year-error" class="error" for="McyearObj.Year"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What is the family?</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.MCfamilyObj.Family, @Model.MCfamilyObj.FamilyList, new { @class = "form-control vlidationInsure form-border-bottom vehicle commondataselection", @id = "family1", @data_eiid = @Model.MCfamilyObj.EiId })
                            @Html.HiddenFor(p => p.MCfamilyObj.EiId)
                            <label id="MCfamilyObj.Family-error" class="error" for="MCfamilyObj.Family"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>Select Correct Details</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.MCscdObj.Scd, Enumerable.Empty<SelectListItem>(), "--Select--", new { @class = "form-control vlidationInsure form-border-bottom vehicle commondataselection", @id = "details1", @data_eiid = @Model.MCscdObj.EiId })
                            @Html.HiddenFor(p => p.MCscdObj.EiId)
                            <label id="MCscdObj.Scd-error" class="error" for="MCscdObj.Scd"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>Estimated retail value *</p></div>
                        <div class="col-md-6">
                            @Html.TextBoxFor(p => p.EstimatedValueObj.EstimatedValue, new { @class = "form-control form-border-bottom vehicle vlidationInsure commondatatest", @id = "estimatedValue", @data_eiid = @Model.EstimatedValueObj.EiId, @data_regex = "N" })
                            @Html.HiddenFor(p => p.EstimatedValueObj.EiId)
                            <label id="EstimatedValueObj.EstimatedValue-error" class="error" for="EstimatedValueObj.EstimatedValue"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <h5>Farm Machinery</h5>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What is the make of your vehicle?</p></div>
                        <div class="col-md-6">
                            @Html.TextBoxFor(p => p.FmmcmakeObj.FmMake, new { @class = "form-control form-border-bottom vehicle vlidationInsure commondatatest", @id = "vehiclemake2", @data_eiid = @Model.FmmcmakeObj.EiId, @data_regex = "A" })
                            @Html.HiddenFor(p => p.FmmcmakeObj.EiId)
                            <label id="FmmcmakeObj.FmMake-error" class="error" for="FmmcmakeObj.FmMake"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What year was it manufactured?</p></div>
                        <div class="col-md-6">
                            @Html.TextBoxFor(p => p.FmmcyearObj.FmYear, new { @class = "form-control form-border-bottom year vehicle vlidationInsure commondatatest", @id = "year2", @data_eiid = @Model.FmmcyearObj.EiId, @readonly = "readonly", @data_regex = "N" })
                            @Html.HiddenFor(p => p.FmmcyearObj.EiId)
                            <label id="FmmcyearObj.FmYear-error" class="error" for="FmmcyearObj.FmYear"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>What is the family?</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.FmmctypeObj.FmFamily, @Model.FmmctypeObj.FmFamilyList, new { @class = "form-control form-border-bottom vehicle vlidationInsure commondataselection", @id = "family2", @data_eiid = @Model.FmmctypeObj.EiId })
                            @Html.HiddenFor(p => p.FmmctypeObj.EiId)
                            <label id="FmmctypeObj.FmFamily-error" class="error" for="FmmctypeObj.FmFamily"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-6"><p>Select Correct Details</p></div>
                        <div class="col-md-6">
                            @Html.DropDownListFor(p => p.FmmcscdObj.FmScd, Enumerable.Empty<SelectListItem>(), "--Select--", new { @class = "form-control form-border-bottom vlidationInsure vehicle commondataselection", @id = "details2", @data_eiid = @Model.FmmcscdObj.EiId })
                            @Html.HiddenFor(p => p.FmmcscdObj.EiId)
                            <label id="FmmcscdObj.FmScd-error" class="error" for="FmmcscdObj.FmScd"></label>
                        </div>
                    </div>
                    <div class="col-md-12"><p class="mini-text">Is the Farm Machinery to be insured operated and maintained in accordance with the manufacturer’s instructions as stated in the owner’s manual?</p></div>
                    <div class="col-md-6 pull-right text-right mt-20">
                        <button type="button" class="button-3d nomargin tab-linker quot-next-btn homedescrtn" id="ptabbtn1">Next</button>
                    </div>
                </div>
                <div id="ptab2">
                    <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 policy-full-head"><h2>ADDITIONAL DESCRIPTION</h2></div>
                    <div class="mt-10 mb-10 col-md-12 ">
                        <p class="col-md-4 col-sm-4 col-xs-12 policy-loc-row">Where is Vehicle normally kept at night?</p>
                        <div class="col-md-8 col-sm-8 col-xs-12">
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '0', new { @class = "commondataradio vlidationInsure", @id = "vehclekept1", @data_eiid = @Model.KeptnightObj.EiId, @checked = "checked" })<label for="radio1"><span><span></span></span>Garage</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '1', new { @class = "commondataradio vlidationInsure", @id = "vehclekept2", @data_eiid = @Model.KeptnightObj.EiId })<label for="radio1"><span><span></span></span>Driveway</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '2', new { @class = "commondataradio vlidationInsure", @id = "vehclekept3", @data_eiid = @Model.KeptnightObj.EiId })<label for="radio1"><span><span></span></span>Carport</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '3', new { @class = "commondataradio vlidationInsure", @id = "vehclekept4", @data_eiid = @Model.KeptnightObj.EiId })<label for="radio1"><span><span></span></span>Street</label></div>
                            <div class="col-md-4 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.KeptnightObj.Keptnight, '4', new { @class = "commondataradio vlidationInsure", @id = "vehclekept5", @data_eiid = @Model.KeptnightObj.EiId })<label for="radio1"><span><span></span></span>Shed</label></div>
                            @Html.HiddenFor(p => p.KeptnightObj.EiId)
                            <label id="KeptnightObj.Keptnight-error" class="error" for="KeptnightObj.Keptnight"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Address</p></div>
                        <div class="col-md-8">
                            @Html.DropDownListFor(p => p.AdaddressObj.Address, @Model.AdaddressObj.AddressList, new { @class = "commondataselection vlidationInsure form-control form-border-bottom", @id = "Address", @data_eiid = @Model.AdaddressObj.EiId })
                            @Html.HiddenFor(p => p.AdaddressObj.EiId)
                            <label id="AdaddressObj.Address-error" class="error" for="AdaddressObj.Address"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Is Vehicle registered?</p></div>
                        <div class="col-md-8">
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.VregisterObj.Register, '1', new { @class = "commondataradio vlidationInsure", @id = "yes", @data_eiid = @Model.VregisterObj.EiId, @checked = "checked" })<label for="radio1"><span><span></span></span>Yes</label></div>
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.VregisterObj.Register, '0', new { @class = "commondataradio vlidationInsure", @id = "no", @data_eiid = @Model.VregisterObj.EiId })<label for="radio1"><span><span></span></span>No</label></div>
                            @Html.HiddenFor(p => p.VregisterObj.EiId)
                            <label id="VregisterObj.Register-error" class="error" for="VregisterObj.Register"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Registration number</p></div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(p => p.RnumberObj.Rnumber, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @id = "rnumber", @data_eiid = @Model.RnumberObj.EiId, @data_regex = "AN" })
                            @Html.HiddenFor(p => p.RnumberObj.EiId)
                            <label id="RnumberObj.Rnumber-error" class="error" for="RnumberObj.Rnumber"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>VIN/chassis number</p></div>
                        <div class="col-md-8">
                            @Html.DropDownListFor(p => p.VnumberObj.Vnumber, @Model.VnumberObj.VnumberList, new { @class = "commondataselection vlidationInsure form-control form-border-bottom", @id = "chassis", @data_eiid = @Model.VnumberObj.EiId })
                            @Html.HiddenFor(p => p.VnumberObj.EiId)
                            <label id="VnumberObj.Vnumber-error" class="error" for="VnumberObj.Vnumber"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Engine number</p></div>
                        <div class="col-md-8">
                            @Html.DropDownListFor(p => p.EnumberObj.Enumber, @Model.EnumberObj.EnumberList, new { @class = "commondataselection vlidationInsure form-control form-border-bottom", @id = "enumber", @data_eiid = @Model.EnumberObj.EiId })
                            @Html.HiddenFor(p => p.EnumberObj.EiId)
                            <label id="EnumberObj.Enumber-error" class="error" for="EnumberObj.Enumber"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>Has Vehicle been modified?</p></div>
                        <div class="col-md-8">
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.VmodifiedObj.Vmodified, '1', new { @class = "commondataradio vlidationInsure", @id = "vehicle1", @data_eiid = @Model.VmodifiedObj.EiId, @checked = "checked" })<label for="radio1"><span><span></span></span>Yes</label></div>
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.RadioButtonFor(p => p.VmodifiedObj.Vmodified, '0', new { @class = "commondataradio vlidationInsure", @id = "vehicle2", @data_eiid = @Model.VmodifiedObj.EiId })<label for="radio1"><span><span></span></span>No</label></div>
                            @Html.HiddenFor(p => p.VmodifiedObj.EiId)
                            <label id="VmodifiedObj.Vmodified-error" class="error" for="VmodifiedObj.Vmodified"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>If yes, please describe modifications</p></div>
                        <div class="col-md-8">
                            @Html.TextBoxFor(p => p.DmodifiedObj.Dmodified, new { @class = "form-control form-border-bottom vlidationInsure commondatatest", @id = "describe", @data_eiid = @Model.DmodifiedObj.EiId, @data_regex = "N" })
                            @Html.HiddenFor(p => p.DmodifiedObj.EiId)
                            <p class="mini-text">(These will require approval from us before accepting your quotation)</p>
                            <label id="DmodifiedObj.Dmodified-error" class="error" for="DmodifiedObj.Dmodified"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>What security features are installed?</p></div>
                        <div class="col-md-8">
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.SFinstalledObj.Installed, new { @class = "commondatacheckbox vlidationInsure", @id = "alarm", @data_eiid = @Model.SFinstalledObj.EiId })<label for="checkbox1"><span><span></span></span>Alarm</label></div>
                            <div class="col-md-6 col-sm-6 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.SFinstalledObj.Installed, new { @class = "commondatacheckbox vlidationInsure", @id = "immobiliser", @data_eiid = @Model.SFinstalledObj.EiId })<label for="checkbox1"><span><span></span></span>Engine immobiliser</label></div>
                            @Html.HiddenFor(p => p.SFinstalledObj.EiId)
                            <label id="SFinstalledObj.Installed-error" class="error" for="SFinstalledObj.Installed"></label>
                        </div>
                    </div>
                    <div class="mt-10 mb-10 col-md-12">
                        <div class="col-md-4"><p>What is the Vehicle used for?</p></div>
                        <div class="col-md-8">
                            <div class="col-md-12 col-sm-12 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.VusedObj.Vused, new { @class = "commondatacheckbox vlidationInsure", @id = "private", @data_eiid = @Model.VusedObj.EiId })<label for="checkbox1"><span><span></span></span>Private Use</label></div>
                            <div class="col-md-12 col-sm-12 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.VusedObj.Vused, new { @class = "commondatacheckbox vlidationInsure", @id = "farm", @data_eiid = @Model.VusedObj.EiId })<label for="checkbox1"><span><span></span></span>Farm use (no farm contracting)</label></div>
                            <div class="col-md-12 col-sm-12 col-xs-12 policy-loc-row">@Html.CheckBoxFor(p => p.VusedObj.Vused, new { @class = "commondatacheckbox vlidationInsure", @id = "business", @data_eiid = @Model.VusedObj.EiId })<label for="checkbox1"><span><span></span></span>Business Use (no farm contracting)</label></div>
                            @Html.HiddenFor(p => p.VusedObj.EiId)
                            <label id="VusedObj.Vused-error" class="error" for="VusedObj.Vused"></label>
                        </div>

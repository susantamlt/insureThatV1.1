            var postcode = suburb1.slice((suburb1.length - 4), suburb1.length);
            var state = suburb1.slice((suburb1.length - 8), (suburb1.length - 5));
            var suburb = suburb1.slice(0, (suburb1.length - 8));
            $.ajax({
                type: "POST",
                url: "/Customer/AddHomeProfile",
                data: { "Add": add, "suburb": suburb, "state": state, "postcode": postcode },
                success: function (res) {
                    if (res.Status) {
                        var locationhb = '' + add + ', ' + suburb1;
                        $('#ptab1 #Address').val(locationhb);
                        $('#newAddressSet').css({ 'display': 'none' });
                    }
                }
            });
        }
    });
